   4902    
   4903   Equation GRAZLAND_PRICE
   4904   # eq'n generates the composite price for sluggish endowments (HT 50) #
   4905   (all,i,AEZ_COMM)(all,r,REG)
   4906       pmgrazland(i,r) = sum(k,GRAZE_INDS, REVSHRP(i,k,r) * pmes(i,k,r));
   4907   !End of pmgrazland!
   4908    
   4909   !Determine pmcrps!
   4910   Coefficient
   4911    SIZE_CRPS # size of pasture and cropland nest set #;
   4912   Formula
   4913    SIZE_CRPS = 2;
   4914    
   4915   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   4916   REVSHRCRPS1(i,r);
   4917   Formula (all,i,AEZ_COMM)(all,r,REG: VOM(i,r) = 0)
   4918   REVSHRCRPS1(i,r) = 1.0 / SIZE_CRPS;
   4919   Formula (all,i,AEZ_COMM)(all,r,REG: VOM(i,r) > 0)
   4920   REVSHRCRPS1(i,r)=sum(j,CROP_INDS,VFM(i,j,r))/sum(k,AGRLAND_INDS, VFM(i,k,r));
   4921    
   4922   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   4923   REVSHRCRPS2(i,r);
   4924   Formula (all,i,AEZ_COMM)(all,r,REG: VOM(i,r) = 0)
   4925   REVSHRCRPS2(i,r) = 1.0 / SIZE_CRPS;
   4926   Formula (all,i,AEZ_COMM)(all,r,REG: VOM(i,r) > 0)
   4927   REVSHRCRPS2(i,r)=sum(j,GRAZE_INDS,VFM(i,j,r))/sum(k,AGRLAND_INDS, VFM(i,k,r));
   4928    
   4929   Equation CRPS_PRICE
   4930   # eq'n generates the composite price for cropland and pasture land #
   4931   (all,i,AEZ_COMM)(all,r,REG)
   4932   pmcrps(i,r) = REVSHRCRPS1(i,r)*(pmcropland(i,r)-afsland(i,r))+
   4933                 REVSHRCRPS2(i,r)*pmgrazland(i,r);
   4934   !End of pmcrps!
   4935    
   4936   !Determine pmland!
   4937   Coefficient
   4938    SIZE_FCRPS # size of Forest abd  CRPS nest set #;
   4939   Formula
   4940    SIZE_FCRPS = 2;
   4941    
   4942   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   4943   REVSHRFCRPS1(i,r);
   4944   Formula (all,i,AEZ_COMM)(all,r,REG: VOM(i,r) = 0)
   4945   REVSHRFCRPS1(i,r) = 1.0 / SIZE_FCRPS;
   4946   Formula (all,i,AEZ_COMM)(all,r,REG: VOM(i,r) > 0)
   4947   REVSHRFCRPS1(i,r)=sum(j,AGRLAND_INDS,VFM(i,j,r))/sum(k,LAND_INDS, VFM(i,k,r));
   4948    
   4949   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   4950   REVSHRFCRPS2(i,r);
   4951   Formula (all,i,AEZ_COMM)(all,r,REG)
   4952   REVSHRFCRPS2(i,r)=1-REVSHRFCRPS1(i,r);
   4953    
   4954   Equation LAND_PRICE
   4955   # eq'n generates the composite price for land #
   4956   (all,i,AEZ_COMM)(all,r,REG)
   4957   pmland(i,r)=REVSHRFCRPS1(i,r)*pmcrps(i,r)+
   4958               REVSHRFCRPS2(i,r)*pmes(i,"forestry",r);
   4959   !End of pmland!
   4960    
   4961   !Bottom of land nest!
   4962   Equation CRPS_SUPPLY
   4963   # eq'n distributes the sluggish endowments across sectors (HT 51) #
   4964   (all,i,AEZ_COMM)(all,r,REG)
   4965    qocrps(i,r) = qo(i,r) - endwslack(i,r)
   4966       + ETRAEL1_1(r) * [pmland(i,r) - pmcrps(i,r)];
   4967    
   4968   Equation OTHLAND_SUPPLY
   4969   # eq'n distributes the sluggish endowments across forestry and grazing #
   4970   (all,i,AEZ_COMM)(all,j,NONAGRLAND)(all,r,REG)
   4971    qoes(i,j,r) =qo(i,r) - endwslack(i,r)
   4972                 + ETRAEL1_1(r) * [pmland(i,r) - pmes(i,j,r)];
   4973   !End of bottom of nest!
   4974    
   4975   !Middle nest of land supply tree!
   4976   Equation CROPLAND_SUPPLY
   4977   # eq'n distributes the sluggish endowments across sectors (HT 51) #
   4978   (all,i,AEZ_COMM)(all,r,REG)
   4979    qocropland(i,r) = qocrps(i,r) -afsland(i,r)- endwslack(i,r) +
   4980                      ETRAEL1_2(r) * [pmcrps(i,r) - pmcropland(i,r)+afsland(i,r)];
   4981    
   4982   Equation GRAZ_SUPPLY
   4983   # eq'n distributes the sluggish endowments across sectors (HT 51) #
   4984   (all,i,AEZ_COMM)(all,r,REG)
   4985    qograzland(i,r) = qocrps(i,r) - endwslack(i,r)
   4986                      + ETRAEL1_2(r) * [pmcrps(i,r) - pmgrazland(i,r)];
   4987   !End of middle nest!
   4988    
   4989   !Top of land supply tree!
   4990    
   4991    
   4992   Equation INDGRAZ_SUPPLY
   4993   # eq'n distributes the sluggish endowments across sectors (HT 51) #
   4994   (all,i,AEZ_COMM)(all,j,GRAZE_INDS)(all,r,REG)
   4995    qoes(i,j,r) = qograzland(i,r) - endwslack(i,r)
   4996       + ETRAEL3 * [pmgrazland(i,r) - pmes(i,j,r)];
   4997    
   4998    
   4999   !<<Begin: Add Crop1 and Crop2 here!
   5000   !<<ETRAEL 2 5 6 were differented by reg ETRAEL4 was deleted  !
   5001   !<<be careful with afsland!
   5002    
   5003   !Revenue share for crops in crop1!
   5004    
   5005   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   5006       CROPRENTS1(i,r);
   5007   Formula (all,i,AEZ_COMM)(all,r,REG)
   5008       CROPRENTS1(i,r) = sum(k,CROP1_INDS, VFM(i,k,r));
   5009   Coefficient
   5010       SIZE_CROP1 # size of CROP1_INDS set #;
   5011   Formula
   5012       SIZE_CROP1 = sum(i,CROP1_INDS, 1);
   5013   Coefficient (all,i,AEZ_COMM)(all,j,CROP1_INDS)(all,r,REG)
   5014       REVSHR4(i,j,r);
   5015   Formula (all,i,AEZ_COMM)(all,j,CROP1_INDS)(all,r,REG: CROPRENTS1(i,r) = 0)
   5016       REVSHR4(i,j,r) = 1.0 / SIZE_CROP1;
   5017   Formula (all,i,AEZ_COMM)(all,j,CROP1_INDS)(all,r,REG: CROPRENTS1(i,r) > 0)
   5018       REVSHR4(i,j,r) = VFM(i,j,r) / sum(k,CROP1_INDS, VFM(i,k,r));
   5019    
   5020   !Revenue share for crops in crop2!
   5021   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   5022       CROPRENTS2(i,r);
   5023   Formula (all,i,AEZ_COMM)(all,r,REG)
   5024       CROPRENTS2(i,r) = sum(k,CROP2_INDS, VFM(i,k,r));
   5025   Coefficient
   5026       SIZE_CROP2 # size of CROP2_INDS set #;
   5027   Formula
   5028       SIZE_CROP2 = sum(i,CROP2_INDS, 1);
   5029   Coefficient (all,i,AEZ_COMM)(all,j,CROP2_INDS)(all,r,REG)
   5030       REVSHR5(i,j,r);
   5031   Zerodivide (zero_by_zero) default 0;
   5032   Formula (all,i,AEZ_COMM)(all,j,CROP2_INDS)(all,r,REG: CROPRENTS2(i,r) = 0)
   5033       REVSHR5(i,j,r) = 1.0 / SIZE_CROP2;
   5034   Formula (all,i,AEZ_COMM)(all,j,CROP2_INDS)(all,r,REG: CROPRENTS2(i,r) > 0)
   5035       REVSHR5(i,j,r) = VFM(i,j,r) / sum(k,CROP2_INDS, VFM(i,k,r));
   5036   Zerodivide (zero_by_zero) off;
   5037    
   5038   !Introduce q and p variables for two crop nests!
   5039   Variable (all,i,AEZ_COMM)(all,r,REG)
   5040       qocropland1(i,r) # supply of sluggish endowment in r (traditional crops)#;
   5041   Variable (all,i,AEZ_COMM)(all,r,REG)
   5042       pmcropland1(i,r) # market price of sluggish end. in r (traditional crops)#;
   5043   Variable (all,i,AEZ_COMM)(all,r,REG)
   5044       qocropland2(i,r) # supply of sluggish endowment in r (mis & pasturecrop)#;
   5045   Variable (all,i,AEZ_COMM)(all,r,REG)
   5046       pmcropland2(i,r) # market price of sluggish end. in r (mis & pasturecrop)#;
   5047    
   5048   !Nesting crop1 and crop2 using ETRAEL2!
   5049    
   5050   Equation CROPLAND_PRICE1
   5051   #eq'n generates the composite price for CROP1_INDS#
   5052   (all,i,AEZ_COMM)(all,r,REG)
   5053       pmcropland1(i,r) = sum(k,CROP1_INDS, REVSHR4(i,k,r) * pmes(i,k,r));
   5054   Equation CROPLAND_SUPPLY1
   5055   # eq'n distributes the slugglish endowments across CROP1 and CROP2 Industries#
   5056   (all,i,AEZ_COMM)(all,r,REG)
   5057       qocropland1(i,r)=qocropland(i,r)+afsland(i,r) - endwslack(i,r)
   5058                    + ETRAEL2(r) * [pmcropland(i,r) - pmcropland1(i,r)];
   5059    
   5060   Equation CROPLAND_PRICE2
   5061   # eq'n generates the composite price for CROP2_INDS#
   5062   (all,i,AEZ_COMM)(all,r,REG)
   5063       pmcropland2(i,r) = sum(k,CROP2_INDS, REVSHR5(i,k,r) * pmes(i,k,r));
   5064   Equation CROPLAND_SUPPLY2
   5065   # eq'n distributes the slugglish endowments across CROP1 and CROP2 Industries#
   5066   (all,i,AEZ_COMM)(all,r,REG)
   5067       qocropland2(i,r)=qocropland(i,r)+afsland(i,r) - endwslack(i,r)
   5068                    + ETRAEL2(r) * [pmcropland(i,r) - pmcropland2(i,r)];
   5069    
   5070    
   5071   !Nesting crop1 using ETRAEL5!				
   5072   Coefficient (parameter)(all,r,REG)
   5073       ETRAEL5(r)
   5074       # elst. of transformation for crop2 land in supply tree #;
   5075   Read
   5076       ETRAEL5 from file GTAPPARL header "ETL5";
   5077    
   5078   Equation INDCROPLAND_SUPPLY1
   5079   # eq'n distributes the sluggish endowments across sectors (HT 51) #
   5080   (all,i,AEZ_COMM)(all,j,CROP1_INDS)(all,r,REG)
   5081    qoes(i,j,r) = qocropland1(i,r) - endwslack(i,r)
   5082       + ETRAEL5(r) * [pmcropland1(i,r) - pmes(i,j,r)];	
   5083    
   5084   !Nesting crop2 using ETRAEL6!
   5085   Coefficient (parameter)(all,r,REG)
   5086       ETRAEL6(r)
   5087       # elst. of transformation for crop2 land in supply tree #;
   5088   Read
   5089       ETRAEL6 from file GTAPPARL header "ETL6";
   5090    
   5091   Equation INDCROPLAND_SUPPLY2
   5092   # eq'n distributes the sluggish endowments across sectors (HT 51) #
   5093   (all,i,AEZ_COMM)(all,j,CROP2_INDS)(all,r,REG)
   5094    qoes(i,j,r) = qocropland2(i,r) - endwslack(i,r)
   5095       + ETRAEL6(r) * [pmcropland2(i,r) - pmes(i,j,r)];	
   5096    
   5097   !<<End two cropland nests!
   5098    
   5099    
   5100    
   5101   !<
   5102   Double Cropping: we moved from del_H=del_Cropland to l=(H/L)*h+(B/L)*b
   5103   Find shift in qocropland due to multiple cropping or cultivating unused land
   5104   >!
   5105    
   5106   !The following is moved from the bottom of code to here!
   5107   Set LCOVER4_INDS # set of land cover categories #
   5108     maximum size 10 read elements from file GTAPSETS header "LAND";
   5109   Set UNMNGLAND # set of unmanaged land #
   5110     maximum size 10 read elements from file GTAPSETS header "UNMG";
   5111   Set PASTURELAND # set of pasture land #
   5112     maximum size 10 read elements from file GTAPSETS header "PAST";
   5113   Subset UNMNGLAND is subset of LCOVER4_INDS ;
   5114   Set LCOVER_INDS # set of land cover categories to be used in model #
   5115           = LCOVER4_INDS - UNMNGLAND;
   5116   Set LCOVER2_INDS # first two land cover categories #
   5117           = FOREST_INDS + PASTURELAND;
   5118   Subset LCOVER2_INDS is subset of LCOVER_INDS ;
   5119   Subset FOREST_INDS is subset of LCOVER_INDS;
Declaration of 'FOREST_INDS' as a SUBSET of 'LCOVER_INDS'.
%% Information only.
    This subset relation is a consequence of earlier subset declarations.
   5120   Set ACROP_INDS # aggregated cropsland cover # = LCOVER_INDS - LCOVER2_INDS ;
   5121   Set NONGRAZ_INDS # non grazing industries # = ACROP_INDS + FOREST_INDS;
   5122   Subset NONGRAZ_INDS is subset of LCOVER_INDS;
   5123   !End  of moving sets!
   5124    
   5125   !The following is moved from the bottom of code to here!
   5126   Variable (LEVELS) (all,i,AEZ_COMM)(all,j,LCOVER4_INDS)(all,r,REG)
   5127       LANDCOVER_L(i,j,r)
   5128       # hectares of land by AEZ, sector and region #;
   5129    
   5130   Read
   5131       LANDCOVER_L from file GTAPDATA header "COVR";
   5132    
   5133   Variable (LEVELS) (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   5134       HARVSTAREA_L(i,j,r)
   5135       # cropland harvested area #;
   5136    
   5137   Read
   5138       HARVSTAREA_L from file GTAPDATA header "AREA";
   5139    
   5140   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   5141       HA_LCSHR(i,r)
   5142       # share of harvested area in total land cover #;
   5143    
   5144   Zerodivide (zero_by_zero) default 1;
   5145   Zerodivide (nonzero_by_zero) default 1;
   5146    
   5147   Formula
   5148   (all,i,AEZ_COMM)(all,r,REG)
   5149       HA_LCSHR(i,r) = sum(k,CROP_INDS, HARVSTAREA_L(i,k,r))/
   5150                       sum(k,ACROP_INDS, LANDCOVER_L(i,k,r)) ;
   5151    
   5152   Zerodivide (zero_by_zero) off ;
   5153   Zerodivide (nonzero_by_zero) off;
   5154    
   5155   !End of move!
   5156    
   5157   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   5158       OTHER(i,r)
   5159       # Area if idled land or double cropping #;
   5160    
   5161   Formula
   5162   (all,i,AEZ_COMM)(all,r,REG)
   5163       OTHER(i,r) = sum(k,ACROP_INDS, LANDCOVER_L(i,k,r))-
   5164                    sum(k,CROP_INDS, HARVSTAREA_L(i,k,r));
   5165    
   5166   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   5167       CROPLAND(i,r)
   5168       # Area of cropland #;
   5169    
   5170   Formula
   5171   (all,i,AEZ_COMM)(all,r,REG)
   5172       CROPLAND(i,r) = sum(k,ACROP_INDS, LANDCOVER_L(i,k,r));
   5173    
   5174   Variable (all,i,AEZ_COMM)(all,r,REG)
   5175       qother(i,r)
   5176       # percent change in area of Iddled-failur-double cropping#;
   5177    
   5178   Coefficient (parameter) (all,i,AEZ_COMM)(all,r,REG)
   5179       GAMA(i,r)
   5180       #Ratio of changes in b_hat/h_hat obtained from historical observations#;
   5181    
   5182   Read
   5183         GAMA from file GTAPPARM header "GAMA";
   5184    
   5185   !The following variable is moved from the bottom of the code!
   5186   Variable (all,i,AEZ_COMM)(all,r,REG)
   5187       harvstaez(i,r)
   5188       # change in crop harvested area within aez #;
   5189    
   5190   !A neative sign is implimented on the RHS of theis equation to have a positive
   5191   shock in land supply when we have double cropping or a reduction in ideled land.
   5192   Notice that to avoid singularity a tiny value (i.e. 1) is added to CROPLAN area
   5193   and OTHER area.!
   5194    
   5195   Equation EAFSLAND
   5196   # Equation to determine percent changes in productivity of supply of land #
   5197   (all,i,AEZ_COMM)(all,r,REG)
   5198    (CROPLAND(i,r)+1)*afsland(i,r)=-(OTHER(i,r)+1)*qother(i,r);
   5199    
   5200   !Start CRP !
   5201    
   5202   Variable (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)        real_pmes(i,j,r)
   5203        # real return to crop land j in region r - useful to monitor CRP lands #;
   5204   Equation REAL_PMESEQ
   5205       # Eq'n defines the real return to crop land j in region r #
   5206       (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   5207       real_pmes(i,j,r) = pmes(i,j,r) - ppriv(r);
   5208    
   5209   Variable (all,j,CRP_LAND)(all,r,REG)                          SUMqoes(j,r)
   5210       # Total CRP type of land (Oth_Ind_Se) in region in r #;
   5211   Equation SUMqoes_EQ
   5212       # Total CRP land in region in r - summed over AEZ #
   5213       (all,j,CRP_LAND)(all,r,REG)
   5214       SUMqoes(j,r) = Sum(i,AEZ_COMM:EVFA(i,j,r)>0, qoes(i,j,r));
   5215    
   5216   !End CRP !
