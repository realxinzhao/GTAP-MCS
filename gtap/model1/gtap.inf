   2221    
   2222    
   2223    
   2224   Equation DMNDDPRICE
   2225   # taxation of intermediate usage of domestic product #
   2226   (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   2227       pfd(i,j,r) = pm(i,r) + tfd(i,j,r);
   2228    
   2229    
   2230   Equation DMNDIPRICES
   2231   # taxation of intermediate usage of imports #
   2232   (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   2233       pfm(i,j,r) = pim(i,r) + tfm(i,j,r);
   2234    
   2235   ! Farzad 2015: BIOFUELS: Separate out the biofuels component of this to permit
   2236       implementation of a RFS !
   2237    
   2238   Variable (change) (all,r,REG) del_taxriunbio(r)
   2239   #change in ratio of tax payments on nbiooil intermed.goods to regional income #;
   2240   Variable (change) (all,r,REG) del_taxriubio(r)
   2241   #change in ratio of tax payments on biooil intermed. goods to regional income #;
   2242    
   2243   Equation TIURATIONBIO
   2244   #change in ratio of tax payments on nbiooil intermed.goods to regional income#
   2245   (all,r,REG)
   2246       100.0 * INCOME(r) * del_taxriunbio(r) + TIUNBIO(r) * y(r)
   2247           = sum(i,NBIOOIL_COMM, sum(j,ALL_INDS,
   2248               VDFA(i,j,r) * tfd(i,j,r) + DFTAX(i,j,r) * [pm(i,r) + qfd(i,j,r)]))
   2249           + sum(i,NBIOOIL_COMM, sum(j,ALL_INDS,
   2250               VIFA(i,j,r) * tfm(i,j,r) + IFTAX(i,j,r) * [pim(i,r) + qfm(i,j,r)]));
   2251    
   2252   Equation TIURATIOBIO
   2253   #change in ratio of tax payments on biooil intermed. goods to regional income#
   2254   (all,r,REG)
   2255       100.0 * INCOME(r) * del_taxriubio(r) + TIUBIO(r) * y(r)
   2256           = sum(i,BIOOIL_COMM, sum(j,ALL_INDS,
   2257               VDFA(i,j,r) * tfd(i,j,r) + DFTAX(i,j,r) * [pm(i,r) + qfd(i,j,r)]))
   2258           + sum(i,BIOOIL_COMM, sum(j,ALL_INDS,
   2259               VIFA(i,j,r) * tfm(i,j,r) + IFTAX(i,j,r) * [pim(i,r) + qfm(i,j,r)]));
   2260    
   2261   Equation TIURATIO
   2262   # change in ratio of tax payments on intermediate goods to regional income #
   2263   (all,r,REG)
   2264    
   2265   del_taxriu(r)
   2266           = del_taxriunbio(r) + del_taxriubio(r);
   2267    
   2268   !Equation TIURATIO
   2269   # change in ratio of tax payments on intermediate goods to regional income #
   2270   (all,r,REG)
   2271       100.0 * INCOME(r) * del_taxriu(r) + TIU(r) * y(r)
   2272           = sum(i,TRAD_COMM, sum(j,ALL_INDS,
   2273               VDFA(i,j,r) * tfd(i,j,r) + DFTAX(i,j,r) * [pm(i,r) + qfd(i,j,r)]))
   2274           + sum(i,TRAD_COMM, sum(j,ALL_INDS,
   2275              VIFA(i,j,r) * tfm(i,j,r) + IFTAX(i,j,r) * [pim(i,r) + qfm(i,j,r)]));!
