   1312   >!
   1313    
   1314   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1315       DGTAX(i,r) # ordinary tax on government consumption of domestic product #;
   1316   Formula (all,i,TRAD_COMM)(all,r,REG)
   1317       DGTAX(i,r) = VDGA(i,r) - VDGM(i,r);
   1318    
   1319   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1320       IGTAX(i,r) # ordinary tax on government consumption of imports #;
   1321   Formula (all,i,TRAD_COMM)(all,r,REG)
   1322       IGTAX(i,r) = VIGA(i,r) - VIGM(i,r);
   1323    
   1324   Coefficient (all,r,REG)
   1325       TGC(r) # ordinary tax on government consumption #;
   1326   Formula (all,r,REG)
   1327       TGC(r) = sum(i,TRAD_COMM, DGTAX(i,r) + IGTAX(i,r));
