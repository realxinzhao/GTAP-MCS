   1280   >!
   1281    
   1282   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1283       DPTAX(i,r) # ordinary tax on private consumption of domestic product #;
   1284   Formula (all,i,TRAD_COMM)(all,r,REG)
   1285       DPTAX(i,r) = VDPA(i,r) - VDPM(i,r);
   1286    
   1287   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1288       IPTAX(i,r) # ordinary tax on private consumption of imports #;
   1289   Formula (all,i,TRAD_COMM)(all,r,REG)
   1290       IPTAX(i,r) = VIPA(i,r) - VIPM(i,r);
   1291    
   1292   ! BIOFUELS: Separate this out for biofuels !
   1293    
   1294   Coefficient (all,r,REG)
   1295       TPCNBIO(r) # ordinary tax on private consumption of non-biooil products #;
   1296   Formula (all,r,REG)
   1297       TPCNBIO(r) = sum(i,NBIOOIL_COMM, DPTAX(i,r) + IPTAX(i,r));
   1298    
   1299   Coefficient (all,r,REG)
   1300       TPCBIO(r) # ordinary tax on private consumption #;
   1301   Formula (all,r,REG)
   1302       TPCBIO(r) = sum(i,BIOOIL_COMM, DPTAX(i,r) + IPTAX(i,r));
   1303    
   1304   Coefficient (all,r,REG)
   1305       TPC(r) # ordinary tax on private consumption #;
   1306   Formula (all,r,REG)
   1307       TPC(r) = TPCNBIO(r) + TPCBIO(r);
   1308    
