   6588       CNTtech_amsr(r) # cont. to EV of bilateral import-augmenting tech change #;
   6589   Equation CONT_EV_tech_amsr
   6590   (all,r,REG)
   6591       CNTtech_amsr(r)
   6592           = [0.01 * EVSCALFACT(r)]
   6593           * sum(i,TRAD_COMM, sum(s,REG, VIMS(i,s,r) * ams(i,s,r)));
   6594    
   6595   Variable (linear,change) (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   6596       CNTtech_amsirs(i,r,s)
   6597    # cont. to EV of bilateral import augmenting tech change for TRAD_COMM i #;
   6598   Equation CONT_EV_tech_amsirs
   6599   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   6600       CNTtech_amsirs(i,r,s) = [0.01 * EVSCALFACT(r)] * VIMS(i,r,s) * ams(i,r,s);
   6601    
   6602   Variable (linear,change) (all,r,REG)
   6603       CNTkbr(r)
   6604       # cont. to EV of changes to beg. period capital stock and depreciation #;
   6605   Equation CONT_EV_kbr
   6606   (all,r,REG)
   6607       CNTkbr(r) = - [0.01 * EVSCALFACT(r)] * VDEP(r) * [kb(r) - pop(r)];
   6608    
   6609    
   6610   !<
   6611       -------------------------------
   6612       D. Terms of Trade Decomposition
   6613       -------------------------------
   6614    
   6615       Computations for decomposition the terms-of-trade effect
   6616       Reference: Robert McDougall, SALTER No. 12 Working Paper
   6617   >!
   6618    
   6619   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   6620       SX_IR(i,r) # share of good i in total exports from r #;
   6621   Formula (all,m,MARG_COMM)(all,r,REG)
   6622       SX_IR(m,r)
   6623           = [sum(s,REG, VXWD(m,r,s)) + VST(m,r)]
   6624           / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s)))
   6625               + sum(l,MARG_COMM, VST(l,r))];
   6626   Formula (all,i,NMRG_COMM)(all,r,REG)
   6627       SX_IR(i,r)
   6628           = sum(s,REG, VXWD(i,r,s))
   6629           / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s)))
   6630               + sum(l,MARG_COMM, VST(l,r))];
   6631    
   6632   Coefficient (all,m,MARG_COMM)(all,s,REG)
   6633       VTICOMM(m,s) # margin usage of m in getting imports to region s #;
   6634   Formula (all,m,MARG_COMM)(all,s,REG)
   6635       VTICOMM(m,s) = sum(i,TRAD_COMM, sum(r,REG, VTMFSD(m,i,r,s)));
   6636    
   6637   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   6638       VIWDIRALL(i,s) # imports of i into s, with direct allocation of margins #;
   6639   Formula (all,m,MARG_COMM)(all,s,REG)
   6640       VIWDIRALL(m,s) = sum(r,REG, VXWD(m,r,s)) + VTICOMM(m,s);
   6641   Formula (all,i,NMRG_COMM)(all,s,REG)
   6642       VIWDIRALL(i,s) = sum(r,REG, VXWD(i,r,s));
   6643    
   6644   Coefficient (all,s,REG)
   6645       VIWDATOT(s) # total imports into s, calculated on direct allocation basis #;
   6646   Formula (all,s,REG)
   6647       VIWDATOT(s) = sum(i,TRAD_COMM, VIWDIRALL(i,s));
   6648    
   6649   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   6650       SM_IR(i,r)  # share of good i in total imports into r #;
   6651   Formula (all,i,TRAD_COMM)(all,s,REG)
   6652       SM_IR(i,s) = VIWDIRALL(i,s) / VIWDATOT(s);
   6653    
   6654   Zerodivide (zero_by_zero) default 0;
   6655   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   6656       SX_IRS(i,r,s) # share of exports of good i from region r to s #;
   6657   Formula (all,m,MARG_COMM)(all,r,REG)(all,s,REG)
   6658       SX_IRS(m,r,s) = VXWD(m,r,s) / [sum(k,REG, VXWD(m,r,k)) + VST(m,r)];
   6659   Formula (all,i,NMRG_COMM)(all,r,REG)(all,s,REG)
   6660       SX_IRS(i,r,s) = VXWD(i,r,s) / sum(k,REG, VXWD(i,r,k));
   6661   Zerodivide (zero_by_zero) off;
   6662    
   6663   Coefficient (all,m,MARG_COMM)(all,r,REG)
   6664       SXT_IR(m,r) # share of margins in exports of good i from region r #;
   6665   Formula (all,m,MARG_COMM)(all,r,REG)
   6666       SXT_IR(m,r) = VST(m,r) / [sum(k,REG, VXWD(m,r,k)) + VST(m,r)];
   6667    
   6668   Variable (all,i,TRAD_COMM)(all,r,REG)
   6669       px_ir(i,r) # export price index for good i and region r #;
   6670   Equation EXPPRICE_MARG
   6671   # price index for total exports of m from r #
   6672   (all,m,MARG_COMM)(all,r,REG)
   6673       px_ir(m,r)
   6674           = sum(s,REG, SX_IRS(m,r,s) * pfob(m,r,s)) + SXT_IR(m,r) * pm(m,r);
   6675   Equation EXPPRICE_NMRG
   6676   # price index for total exports of i from r #
   6677   (all,i,NMRG_COMM)(all,r,REG)
   6678       px_ir(i,r) = sum(s,REG, SX_IRS(i,r,s) * pfob(i,r,s));
   6679    
   6680    
   6681   Zerodivide (zero_by_zero) default 0;
   6682   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   6683       SM_IRS(i,r,s) # share of imports of good i into s from r at FOB prices #;
   6684   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   6685       SM_IRS(i,r,s) = VXWD(i,r,s) / VIWDIRALL(i,s);
   6686    
   6687    
   6688   Coefficient (all,m,MARG_COMM)(all,r,REG)
   6689       SMT_IR(m,r) # share of transport cost in imports of margin commodity #;
   6690   Formula (all,m,MARG_COMM)(all,s,REG)
   6691       SMT_IR(m,s) = VTICOMM(m,s) / VIWDIRALL(m,s);
   6692    
   6693   Variable (all,i,TRAD_COMM)(all,r,REG)
   6694       pm_ir(i,r) # imports price index for good i and region r #;
   6695   Equation IMPPRICE1_MARG
   6696   # price index for total imports of m in s -- margins commodities #
   6697   (all,m,MARG_COMM)(all,s,REG)
   6698       pm_ir(m,s)
   6699           = sum(r,REG, SM_IRS(m,r,s) * pfob(m,r,s)) + SMT_IR(m,s) * pt(m);
   6700   Equation IMPPRICE1_NMRG
   6701   # price index for total imports of i in s -- non-margins commodities #
   6702   (all,i,NMRG_COMM)(all,s,REG)
   6703       pm_ir(i,s) = sum(r,REG, SM_IRS(i,r,s) * pfob(i,r,s));
   6704    
   6705   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   6706       SW_IR(i,r) # share of region r exports in world total for good i #;
   6707   Formula (all,m,MARG_COMM)(all,r,REG)
   6708       SW_IR(m,r)
   6709           = [sum(s,REG, VXWD(m,r,s)) + VST(m,r)]
   6710           / sum(k,REG, sum(s,REG, VXWD(m,k,s)) + VST(m,k));
   6711   Formula (all,i,NMRG_COMM)(all,r,REG)
   6712       SW_IR(i,r)
   6713           = sum(s,REG, VXWD(i,r,s)) / sum(k,REG, sum(s,REG, VXWD(i,k,s)));
   6714    
   6715   Variable (all,i,TRAD_COMM)
   6716       px_i(i) # world export price index for commodity i #;
   6717   Equation WRLDPRICEi
   6718   # world export price index for good i #
   6719   (all,i,TRAD_COMM)
   6720       px_i(i) = sum(r,REG, SW_IR(i,r) * px_ir(i,r));
   6721    
   6722   Coefficient (all,i,TRAD_COMM)
   6723       SW_I(i) # share of exports of i in world total #;
   6724   Formula (all,m,MARG_COMM)
   6725       SW_I(m)
   6726           = sum(r,REG, sum(s,REG, VXWD(m,r,s)) + VST(m,r))
   6727           / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s))))
   6728               + sum(l,MARG_COMM, sum(r,REG, VST(l,r)))];
   6729   Formula (all,i,NMRG_COMM)
   6730       SW_I(i)
   6731           = sum(r,REG, sum(s,REG, VXWD(i,r,s)))
   6732           / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s))))
   6733               + sum(l,MARG_COMM, sum(r,REG, VST(l,r)))];
   6734   Zerodivide (zero_by_zero) off;
   6735   Variable
   6736       px_ # world export price index for all commodities #;
   6737   Equation WRLDPRICE
   6738   # world export price index for all goods #
   6739       px_ = sum(i,TRAD_COMM, SW_I(i) * px_i(i));
   6740    
   6741   Variable (all,i,TRAD_COMM)(all,r,REG)
   6742       c1_ir(i,r) # contribution of world price, px_i, to ToT #;
   6743   Equation c1_irEQ
   6744   # contribution of world export price index of good i to ToT for region r #
   6745   (all,i,TRAD_COMM)(all,r,REG)
   6746       c1_ir(i,r) = [SX_IR(i,r) - SM_IR(i,r)] * [px_i(i) - px_];
   6747    
   6748   Variable (all,i,TRAD_COMM)(all,r,REG)
   6749       c2_ir(i,r) # contribution of regional export price, px_ir, to ToT #;
   6750   Equation c2_irEQ
   6751   # contribution of regional export price of good i for region r #
   6752   (all,i,TRAD_COMM)(all,r,REG)
   6753       c2_ir(i,r) = SX_IR(i,r) * [px_ir(i,r) - px_i(i)];
   6754    
   6755   Variable (all,i,TRAD_COMM)(all,r,REG)
   6756       c3_ir(i,r) # contribution of regional import price, pm_ir, to ToT #;
   6757   Equation c3_irEQ
   6758   # contribution of imports price index of good i for region r #
   6759   (all,i,TRAD_COMM)(all,r,REG)
   6760       c3_ir(i,r) = SM_IR(i,r) * [pm_ir(i,r) - px_i(i)];
   6761    
   6762   Variable (all,r,REG)
   6763       c1_r(r) # contribution of world prices for all goods to ToT #;
   6764   Equation c1_rEQ
   6765   # contribution of world price indexes of all goods to ToT for r #
   6766   (all,r,REG)
   6767       c1_r(r) = sum(i,TRAD_COMM, c1_ir(i,r));
   6768    
   6769   Variable (all,r,REG)
   6770       c2_r(r) # contribution of regional export prices to ToT #;
   6771   Equation c2_rEQ
   6772   # contribution of regional exports prices to ToT for r #
   6773   (all,r,REG)
   6774       c2_r(r) = sum(i,TRAD_COMM, c2_ir(i,r));
   6775    
   6776   Variable (all,r,REG)
   6777       c3_r(r) # contribution of regional import prices to ToT #;
   6778   Equation c3_rEQ
   6779   # contribution of regional import prices to ToT for r #
   6780   (all,r,REG)
   6781       c3_r(r) = sum(i,TRAD_COMM, c3_ir(i,r));
   6782    
   6783   Variable (all,r,REG)
   6784       tot2(r) # trade terms for region r, computed from components #;
   6785   Equation TOT2eq
   6786   # trade terms for region r, computed from components #
   6787   (all,r,REG)
   6788       tot2(r) = c1_r(r) + c2_r(r) - c3_r(r);
   6789    
   6790    
   6791   !<
   6792       ------------------
   6793       E. Land Use Module
   6794       ------------------
   6795       Computes changes in land cover and harvested area in hectares.
   6796       Land cover refers to forestry, grazing and agriculture.
   6797       Land use in crops is based on harvested area.
   6798    
   6799   In order to reconcile market clearing in hectares with that on a
   6800   value basis, as dictated by the CET function, we require an adjustment factor
   6801   in each of the AEZs. This adjusts the percentage change in hectares to
   6802   reflect the average change in productivity of those hectares in their new
   6803   uses.
   6804    
   6805   >!
   6806    
   6807   !Double crroping: the following moved up!
   6808    
   6809   !Set LCOVER4_INDS # set of land cover categories #
   6810     maximum size 10 read elements from file GTAPSETS header "LAND";
   6811   Set UNMNGLAND # set of unmanaged land #
   6812     maximum size 10 read elements from file GTAPSETS header "UNMG";
   6813   Set PASTURELAND # set of pasture land #
   6814     maximum size 10 read elements from file GTAPSETS header "PAST";
   6815   Subset UNMNGLAND is subset of LCOVER4_INDS ;
   6816   Set LCOVER_INDS # set of land cover categories to be used in model #
   6817           = LCOVER4_INDS - UNMNGLAND;
   6818   Set LCOVER2_INDS # first two land cover categories #
   6819           = FOREST_INDS + PASTURELAND;
   6820   Subset LCOVER2_INDS is subset of LCOVER_INDS ;
   6821   Subset FOREST_INDS is subset of LCOVER_INDS;
   6822   Set ACROP_INDS # aggregated cropsland cover # = LCOVER_INDS - LCOVER2_INDS ;
   6823   Set NONGRAZ_INDS # non grazing industries # = ACROP_INDS + FOREST_INDS;
   6824   Subset NONGRAZ_INDS is subset of LCOVER_INDS;!
   6825    
   6826   Variable (all,i,AEZ_COMM)(all,r,REG)
   6827       lcoveraez(i,r)
   6828       # change in land in aez -- generally a fixed endowment #;
   6829    
   6830   ! Make LANDCOVER a levels variable -- and exogenize landcover change in
   6831   unmanaged land. !
   6832    
   6833   !Double cropping:the following moved up!
   6834   !Variable (LEVELS) (all,i,AEZ_COMM)(all,j,LCOVER4_INDS)(all,r,REG)
   6835       LANDCOVER_L(i,j,r)
   6836       # hectares of land by AEZ, sector and region #;
   6837   Read
   6838       LANDCOVER_L from file GTAPDATA header "COVR";!
   6839    
   6840   Zerodivide (zero_by_zero) default 1;
   6841   Zerodivide (nonzero_by_zero) default 1;
   6842    
   6843   Coefficient (all,i,AEZ_COMM)(all,j,LAND_INDS)(all,r,REG)
   6844       NVFM(i,j,r)
   6845       # land rental payments in $ #;
   6846   Formula
   6847   (all,i,AEZ_COMM)(all,j,LAND_INDS)(all,r,REG)
   6848       NVFM(i,j,r) = VFM(i,j,r) * 10^6 ;
   6849    
   6850   Variable (LEVELS) (all,i,AEZ_COMM)(all,j,LCOVER_INDS)(all,r,REG)
   6851       RENTCOVER(i,j,r)
   6852       # level of land rents by AEZ, sector and region, in $/ha. #;
   6853   Formula (initial)
   6854   (all,i,AEZ_COMM)(all,j,FOREST_INDS)(all,r,REG)
   6855       RENTCOVER(i,j,r) = [NVFM(i,j,r)/LANDCOVER_L(i,j,r)] ;
   6856   Formula (initial)
   6857   (all,i,AEZ_COMM)(all,j,PASTURELAND)(all,r,REG)
   6858       RENTCOVER(i,j,r) = sum(k,GRAZE_INDS,NVFM(i,k,r))/LANDCOVER_L(i,j,r) ;
   6859   Formula (initial)
   6860   (all,i,AEZ_COMM)(all,j,ACROP_INDS)(all,r,REG)
   6861       RENTCOVER(i,j,r) = [sum(k,CROP_INDS, NVFM(i,k,r))/LANDCOVER_L(i,j,r)] ;
   6862    
   6863    
   6864   ! The change in land rents per physical hectare is computed by noting that
   6865   total land rental payments, by cover type and AEZ, must be the same, regardless
   6866   of units of measurement for land. !
   6867    
   6868   Equation E_RENTCOVER1
   6869   (all,i,AEZ_COMM)(all,j,FOREST_INDS)(all,r,REG)
   6870       p_RENTCOVER(i,j,r) + p_LANDCOVER_L(i,j,r) = pmes(i,j,r) + qoes(i,j,r) ;
   6871    
   6872   Equation E_RENTCOVER2
   6873   (all,i,AEZ_COMM)(all,j,PASTURELAND)(all,r,REG)
   6874       p_RENTCOVER(i,j,r) + p_LANDCOVER_L(i,j,r)
   6875                   = pmgrazland(i,r) + qograzland(i,r) ;
   6876    
   6877   Equation E_RENTCOVER3
   6878   (all,i,AEZ_COMM)(all,j,ACROP_INDS)(all,r,REG)
   6879       p_RENTCOVER(i,j,r) + p_LANDCOVER_L(i,j,r) =
   6880                           pmcropland(i,r) + qocropland (i,r);
   6881    
   6882   Variable (LEVELS) (all,i,AEZ_COMM)(all,r,REG)
   6883       RENTCOVERAV(i,r)
   6884       # level of average land rents by AEZ and region #;
   6885    
   6886   Formula (initial)
   6887   (all,i,AEZ_COMM)(all,r,REG)
   6888       RENTCOVERAV(i,r) =
   6889     [sum(j,LAND_INDS, NVFM(i,j,r))/sum(j,LCOVER_INDS, LANDCOVER_L(i,j,r))] ;
   6890    
   6891   Equation E_RENTCOVERAV
   6892   (all,i,AEZ_COMM)(all,r,REG)
   6893       p_RENTCOVERAV(i,r) + lcoveraez(i,r) = pm(i,r) + qo(i,r) ;
   6894    
   6895   !Double cropping:The following moved up!
   6896   !Variable (LEVELS) (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   6897       HARVSTAREA_L(i,j,r)
   6898       # cropland harvested area #;
   6899   Read
   6900       HARVSTAREA_L from file GTAPDATA header "AREA";!
   6901    
   6902   ! New coefficient for deflating the growth in land cover certain regions. The
   6903     problem is that there is a great deal of crop land cover that is not
   6904     cropped for one reason or another (e.g., CRP land in USA). Therefore, we
   6905     do not expect crop land cover to expand in the same proportion as
   6906     harvested area. Instead, we keep the absolute change in these two levels
   6907     variables the same. So if the model calls for 10 million hectares more
   6908     of harvested area, land cover must also rise by this amount. !
   6909    
   6910   !Double cropping: the following moved up!
   6911   !Coefficient (all,i,AEZ_COMM)(all,r,REG)
   6912       HA_LCSHR(i,r)
   6913       # share of harvested area in total land cover #;
   6914   Formula
   6915   (all,i,AEZ_COMM)(all,r,REG)
   6916       HA_LCSHR(i,r) =
   6917   sum(k,CROP_INDS, HARVSTAREA_L(i,k,r))/sum(k,ACROP_INDS, LANDCOVER_L(i,k,r)) ;!
   6918    
   6919    
   6920   !<
   6921       --------------------------------
   6922       PRODUCTIVITY SENSITIVITY VERSION
   6923       --------------------------------
   6924    
   6925   - This version specifies an elasticity of effective crop land wrt land crop land
   6926   expansion: ETA. ETA is the percentage change in effective cropland for a given
   6927   percentage change in crop land area. So, if ETA is 0.5, then to achieve a
   6928   one percent expansion in effective cropland, we need a two percent rise in
   6929   harvested cropland area. However, since a one percent rise in harvested cropland
   6930   does not require a one percent rise in total land cover, we also pre-multiply
   6931   by the ratio of harvested area to land cover. Thus, if ETA = 0.5 and HA is
   6932   just 2/3's of land cover, then crop land cover will rise by 4/3's of the rise in
   6933   harvested crop land requirements. Mathematically, we have:
   6934    
   6935   ETA = %chng effective cropland / %change harvetsed cropland area
   6936   qocropland = our measure of % change in effective cropland
   6937   cropland = % change in cropland area, which is NOT the same as cropland cover.
   6938    
   6939   Thus:
   6940   ETA = qocropland/cropland
   6941    
   6942   The problem arises in determining the percentage change: cropland.
   6943    
   6944   First of all, cropland is based on harvested area (HA), not physically
   6945   cultivated area (PA). When there are extensive crop failures, HA < PA. However,
   6946   when multiple cropping is present, we have HA > PA. We do not have global crop
   6947   data on PA.
   6948    
   6949   Secondly, the data bases on crop land cover (LC) and PA are quite different.
   6950   In the US, cropland cover includes crop land used for pasture, idle land and
   6951   CRP land.The difference is huge.
   6952    
   6953   Combining both of these problems, we are left with a large, inexplicable, gap
   6954   between LC and HA. Since LC >> HA in most cases, assuming that both move in
   6955   equal proportions greatly inflates the total land cover requirements for a given
   6956   percentage change in HA. For this reason, we have reverted to the assumption
   6957   that del_LC = del_HA instead.
   6958    
   6959   In percentage changes, we have:
   6960    
   6961   total land cover = (HA/LC)*harvested area + (Other/LC)*other crop land cover
   6962    
   6963   If other crop land cover (e.g., idle land) does not change, then harvested
   6964   area is simply scaled by HA/LC to give the change in total land cover.
   6965    
   6966   >!
   6967    
   6968   Coefficient (parameter) (all,i,AEZ_COMM)(all,r,REG)
   6969       ETA(i,r)
   6970       # elasticity of output with respect to acreage expansion #;
   6971   Read
   6972       ETA from file GTAPPARM header "ETA";
   6973    
   6974   ! Now provide a revised estimate of the crop land area required, based
   6975   on ETA, the elasticity of effective crop land wrt harvested area. !
   6976    
   6977   ! Now switch to the harvested area portion of the code. !
   6978    
   6979   !Double cropping: the following moved up!
   6980   !Variable (all,i,AEZ_COMM)(all,r,REG)
   6981       harvstaez(i,r)
   6982       # change in crop harvested area within aez #;!
   6983    
   6984   Variable (all,i,AEZ_COMM)(all,r,REG)
   6985       harvstslack(i,r)
   6986       # slack variable in land allocation condition #;
   6987   Variable (all,j,CROP_INDS)(all,r,REG)
   6988       harvstcom(j,r)
   6989       # aggregates harvested area across AEZs for a given commodity, by region #;
   6990    
   6991   Equation E_HARVSTAEZ
   6992   (all,i,AEZ_COMM)(all,r,REG)
   6993       harvstaez(i,r) = [1/ETA(i,r)] * (qocropland (i,r) + afsland(i,r));
   6994    
   6995   ! Non-crop cover adjusts to accommodate this difference, so areaadjust
   6996   must change to satisfy the constant hectares condition for each AEZ. !
   6997    
   6998   Variable (all,i,AEZ_COMM)(all,r,REG)
   6999       areaadjust(i,r)
   7000       # adjusts noncrop areas to satisfy constant hectares per AEZ #;
   7001    
   7002   Equation E_NONCROPCOV1
   7003   (all,i,AEZ_COMM)(all,j,FOREST_INDS)(all,r,REG)
   7004       p_LANDCOVER_L(i,j,r) = qoes(i,j,r) + areaadjust(i,r) ;
   7005    
   7006   Equation E_NONCROPCOV2
   7007   (all,i,AEZ_COMM)(all,j,PASTURELAND)(all,r,REG)
   7008       p_LANDCOVER_L(i,j,r) = qograzland(i,r) + areaadjust(i,r) ;
   7009    
   7010   Coefficient (all,i,AEZ_COMM)(all,j,LCOVER_INDS)(all,r,REG)
   7011       LCOVERSHR(i,j,r)
   7012       # share in land cover #;
   7013   Formula
   7014   (all,i,AEZ_COMM)(all,j,LCOVER_INDS)(all,r,REG)
   7015       LCOVERSHR(i,j,r) =
   7016           LANDCOVER_L(i,j,r)/sum(k,LCOVER_INDS, LANDCOVER_L(i,k,r)) ;
   7017    
   7018   Equation FIXCOVER
   7019   # eq'n scales qoes to ensure constant hectares #
   7020   (all,i,AEZ_COMM)(all,r,REG)
   7021       lcoveraez(i,r) =
   7022           sum(j,LCOVER_INDS, LCOVERSHR(i,j,r) * p_LANDCOVER_L(i,j,r));
   7023    
   7024   !commenting here out for MCET !
   7025   !Equation E_AREAADJUST
   7026   # links total land cover in the AEZ to the endowment #
   7027   (all,i,AEZ_COMM)(all,r,REG)
   7028       lcoveraez(i,r) = qo(i,r); !
   7029    
   7030    
   7031   ! Compute the change in national land cover, by type !
   7032    
   7033   Variable (all,j,LCOVER_INDS)(all,r,REG)
   7034       lcovercom(j,r)
   7035       # change in regional land cover #;
   7036    
   7037   Coefficient (all,i,AEZ_COMM)(all,j,LCOVER_INDS)(all,r,REG)
   7038       COMLCOVERSHR(i,j,r)
   7039       # share in land cover #;
   7040   Formula
   7041   (all,i,AEZ_COMM)(all,j,LCOVER_INDS)(all,r,REG)
   7042       COMLCOVERSHR(i,j,r) =
   7043   LANDCOVER_L(i,j,r)/sum(m,AEZ_COMM, LANDCOVER_L(m,j,r)) ;
   7044    
   7045   Equation COMCOVER
   7046   # eq'n computes regional land cover change #
   7047   (all,j,LCOVER_INDS)(all,r,REG)
   7048       lcovercom(j,r) =
   7049    sum(m,AEZ_COMM, COMLCOVERSHR(m,j,r) * p_LANDCOVER_L(m,j,r));
   7050    
   7051   Variable (all,j,LCOVER_INDS)
   7052       globlcovercom(j)
   7053       # change in global land cover #;
   7054   Equation GLCOMCOVER
   7055    # change in global land cover #
   7056   (all,j,LCOVER_INDS)
   7057       globlcovercom(j) = sum(r,REG,(sum(i, AEZ_COMM,LANDCOVER_L(i,j,r))
   7058           /sum(k, REG,sum(i,AEZ_COMM, LANDCOVER_L(i,j,k))))* lcovercom(j,r));
   7059    
   7060   Variable (all,j,LCOVER_INDS)
   7061       nuslcovercom(j)
   7062       # change in non US land cover #;
   7063   Equation NUSCOMCOVER
   7064    # change in non US land cover #
   7065   (all,j,LCOVER_INDS)
   7066       nuslcovercom(j) = sum(r,NUS_REG,(sum(i, AEZ_COMM,LANDCOVER_L(i,j,r))
   7067           /sum(k, NUS_REG,sum(i,AEZ_COMM, LANDCOVER_L(i,j,k))))* lcovercom(j,r));
   7068    
   7069   Variable (LEVELS) (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7070       RENTCROP(i,j,r)
   7071       # level of land rents by AEZ, sector and region, in $/ha. #;
   7072    
   7073   Formula (initial)
   7074   (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7075       RENTCROP(i,j,r) = [NVFM(i,j,r)/HARVSTAREA_L(i,j,r)]  ;
   7076    
   7077   Equation E_RENTCROP1
   7078   (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7079       p_RENTCROP(i,j,r) + p_HARVSTAREA_L(i,j,r) = pmes(i,j,r) + qoes(i,j,r) ;
   7080    
   7081   Variable (LEVELS) (all,i,AEZ_COMM)(all,r,REG)
   7082       RENTCROPAV(i,r)
   7083       # level of average crop land rents by AEZ and region #;
   7084    
   7085   Formula (initial)
   7086   (all,i,AEZ_COMM)(all,r,REG)
   7087       RENTCROPAV(i,r) =
   7088     [sum(j,CROP_INDS, NVFM(i,j,r))/sum(j,CROP_INDS, HARVSTAREA_L(i,j,r))] ;
   7089    
   7090   Equation E_RENTCROPAV
   7091   (all,i,AEZ_COMM)(all,r,REG)
   7092       p_RENTCROPAV(i,r) + harvstaez(i,r) = pmcropland(i,r) + qocropland(i,r) ;
   7093    
   7094   Equation E_HARVSTAREA
   7095   # eq'n scales qoes to get revised p_HA and ensure constant physical hectares #
   7096   (all,i,AEZ_COMM)(all,j,NCRP_INDS)(all,r,REG)
   7097       p_HARVSTAREA_L(i,j,r) =
   7098           harvstslack(i,r) + qoes(i,j,r) ;
   7099    
   7100   Coefficient (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7101       HAREASHR(i,j,r)
   7102       # share of crop j in total AEZ i land area #;
   7103    
   7104   Formula
   7105   (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7106       HAREASHR(i,j,r) =
   7107           HARVSTAREA_L(i,j,r)/sum(k,CROP_INDS, HARVSTAREA_L(i,k,r)) ;
   7108    
   7109   Equation FIXAREA
   7110   # eq'n scales qoes to ensure constant hectares #
   7111   (all,i,AEZ_COMM)(all,r,REG)
   7112       harvstaez(i,r) =
   7113           sum(j,CROP_INDS, HAREASHR(i,j,r) * p_HARVSTAREA_L(i,j,r));
   7114    
   7115   !Farzad: Double Cropping: I moved from del_H=del_Cropland to l=(H/L)*h+(B/L)*b!
   7116    
   7117   Equation CROPCOVER1
   7118   # total harvested area is tied to the endowment of cropland cover  #
   7119   (all,i,AEZ_COMM)(all,j,ACROP_INDS)(all,r,REG)
   7120       p_LANDCOVER_L(i,j,r) = GAMA(i,r)*(HA_LCSHR(i,r)*harvstaez(i,r));
   7121    
   7122   Equation CROPCOVER2
   7123   # total harvested area is tied to the endowment of cropland cover  #
   7124   (all,i,AEZ_COMM)(all,j,ACROP_INDS)(all,r,REG)
   7125       p_LANDCOVER_L(i,j,r) = (HA_LCSHR(i,r) * harvstaez(i,r)+
   7126                              (1-HA_LCSHR(i,r)+.00001)*qother(i,r));
   7127    
   7128   Coefficient (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7129       AEZHAREASHR(i,j,r)
   7130       # share of AEZ i in total crop j land area #;
   7131   Formula
   7132   (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7133       AEZHAREASHR(i,j,r) =
   7134           HARVSTAREA_L(i,j,r)/sum(k,AEZ_COMM, HARVSTAREA_L(k,j,r)) ;
   7135    
   7136   Equation COMHARVEST
   7137   # eq'n scales qoes to ensure constant hectares #
   7138   (all,j,CROP_INDS)(all,r,REG)
   7139       harvstcom(j,r) =
   7140           sum(i,AEZ_COMM, AEZHAREASHR(i,j,r) * p_HARVSTAREA_L(i,j,r));
   7141    
   7142   Variable (all,j,CROP_INDS)
   7143       globharvstcom(j)
   7144       # global harvested area#;
   7145   Equation GLCOMHARVEST
   7146   #global harvested area#
   7147   (all,j,CROP_INDS)
   7148       globharvstcom(j) = sum(r, REG,sum(i,AEZ_COMM,HARVSTAREA_L(i,j,r))
   7149           /sum(m,REG,sum(k,AEZ_COMM, HARVSTAREA_L(k,j,m)))* harvstcom(j,r));
   7150   !
   7151   Variable (all,j,CROP_INDS)
   7152       nusharvstcom(j)
   7153       # non US  harvested area#;
   7154   Equation NUSCOMHARVEST
   7155   # non US harvested area#
   7156   (all,j,CROP_INDS)
   7157       nusharvstcom(j) = sum(r, NUS_REG,sum(i,AEZ_COMM,HARVSTAREA_L(i,j,r))
   7158          /sum(m,NUS_REG,sum(k,AEZ_COMM, HARVSTAREA_L(k,j,m)))* harvstcom(j,r));!
   7159    
   7160    
   7161   ! FYI: also compute the change national average crop yield due to changes
   7162   in crop land area !
   7163    
   7164   Variable (all,r,REG)
   7165       hareaslackreg(r)
   7166       # national yield adjustment #;
   7167    
   7168   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   7169       AEZHREGSHR(i,r)
   7170       # AEZ share in regional land cover #;
   7171   Formula
   7172   (all,i,AEZ_COMM)(all,r,REG)
   7173       AEZHREGSHR(i,r) =
   7174   sum(j,CROP_INDS, HARVSTAREA_L(i,j,r))/
   7175           sum(m,AEZ_COMM, sum(j,CROP_INDS, HARVSTAREA_L(m,j,r))) ;
   7176    
   7177   Equation HARVCOVERSLACK
   7178   # eq'n computes regional land cover change #
   7179   (all,r,REG)
   7180       hareaslackreg(r) =
   7181    sum(i,AEZ_COMM, AEZHREGSHR(i,r) * harvstslack(i,r));
   7182    
   7183   ! Finally, compute the productivity-weighted change in land cover. This
   7184   should be zero. !
   7185    
   7186   Variable (all,i,AEZ_COMM)(all,r,REG)
   7187       xlcoveraez(i,r)
   7188   # change in productivity-weighted land cover, by aez # ;
   7189   Equation E_XLCOVERAEZ
   7190   # change in productivity-weighted land cover, by aez #
   7191   (all,i,AEZ_COMM)(all,r,REG)
   7192   xlcoveraez(i,r) = sum(j,FOREST_INDS, REVSHR1(i,j,r) * qoes(i,j,r))
   7193                   + [sum(j,GRAZE_INDS, REVSHR1(i,j,r))] * qograzland(i,r)
   7194                   + [sum(j,CROP_INDS, REVSHR1(i,j,r))] * qocropland(i,r);
   7195    
   7196    
   7197    
   7198   !<
   7199       ---------------------------
   7200       F. BIOFUEL TARGETING MODULE
   7201       ---------------------------
   7202    
   7203   - Can track the volume of biofuel output for a key few INDSustries. User must
   7204    specify and read in initial level (all others will be simply set equal to one).
   7205    
   7206   - Also we want to track the share of renewables in liquid fuels.
   7207    
   7208   >!
   7209   !<< modified!
   7210   Set
   7211       BIOFUEL_COMM # renewable inputs into biooil #
   7212       (ethanol1,ethanol2,ethanol3,biod_soy,biod_palm,biod_rape,
   7213        biod_oth,GRNATJ_J,GRNETJ_J,SUGATJ_J,SUGETJ_J,SUGSIP_J,SOYHRJ,RAPHRJ,PLMHRJ,CAMHRJ_J,
   7214       CRNHRJ_J,MSCFTJ,SWGFTJ,POPFTJ,MSCATJ,SWGATJ);
   7215    
   7216    
   7217   Subset
   7218       BIOFUEL_COMM is subset of BIOOIL_COMM;
   7219    
   7220   Set
   7221       BIOOIL1_COMM # Inputs into blend #
   7222       (Oil_Pcts,ethanol1,ethanol2,ethanol3,biod_soy,biod_palm,biod_rape,
   7223        biod_oth,GRNATJ_J,GRNETJ_J,SUGATJ_J,SUGETJ_J,SUGSIP_J,SOYHRJ,RAPHRJ,PLMHRJ,CAMHRJ_J,
   7224       CRNHRJ_J,MSCFTJ,SWGFTJ,POPFTJ,MSCATJ,SWGATJ);
   7225    
   7226   Subset
   7227       BIOOIL1_COMM is subset of BIOOIL_COMM;
   7228    
   7229   Subset
   7230       BIOFUEL_COMM is subset of BIOOIL1_COMM;
   7231   !
   7232   VARIABLE (LEVELS) (all,i,BIOOIL1_COMM)(all,r,REG)
   7233       BIOOIL(i,r)
   7234       # volume of biofuels usage in millions of gallons #;
   7235   Read
   7236       BIOOIL from file GTAPDATA header "BOIL";
   7237    
   7238   COEFFICIENT  (all,i,BIOOIL1_COMM)(all,r,REG)
   7239   TOTUSE(i,r)  # total use of fuels in blend #;
   7240   FORMULA
   7241   (all,i,BIOOIL1_COMM)(all,r,REG)
   7242       TOTUSE(i,r) =
   7243       VIFM(i,"Blender",r) + VDFM(i,"Blender",r);
   7244    
   7245   Variable (all,i,BIOOIL1_COMM)(all,r,REG)
   7246       biooiluse(i,r)
   7247       # change in volume of biooils usage #;
   7248   Equation E_BIOUSE
   7249   (all,i,BIOOIL1_COMM)(all,r,REG)
   7250      TOTUSE(i,r)*biooiluse(i,r) =[VIFM(i,"Blender",r) * qfm(i,"Blender",r)+
   7251                       VDFM(i,"Blender",r) * qfd(i,"Blender",r)];
   7252    
   7253   Equation E_BIOOIL
   7254   (all,i,BIOOIL1_COMM)(all,r,REG)
   7255       p_BIOOIL(i,r) = biooiluse(i,r);
   7256    
   7257   Variable (LEVELS, Change) (all,i,BIOOIL1_COMM)(all,r,REG)
   7258       SHRRENEWi(i,r)
   7259       # share of inidividual renewables in total liquid fuels #;
   7260   Formula & Equation (LEVELS)E_SHRRENEWi (all,i,BIOOIL1_COMM)(all,r,REG)
   7261       SHRRENEWi(i,r) =
   7262    BIOOIL(i,r)/sum(k,BIOOIL1_COMM, BIOOIL(k,r));
   7263    
   7264   Variable (LEVELS, Change) (all,r,REG)
   7265       SHRRENEW(r)
   7266       # share of renewables in total liquid fuels #;
   7267   Formula & Equation (LEVELS)E_SHRRENEW (all,r,REG)
   7268       SHRRENEW(r) =
   7269    sum(i,BIOFUEL_COMM, BIOOIL(i,r))/sum(i,BIOOIL1_COMM, BIOOIL(i,r));
   7270    
   7271   Variable (all,r,REG)
   7272       biofueluse(r)
   7273       # change in volume of total biofuels (excluing petroleum) usage #;
   7274   Equation E_BIOFUELUSE
   7275   (all,r,REG)
   7276       sum(i,BIOFUEL_COMM, TOTUSE(i,r)) * biofueluse(r) =
   7277        sum(i,BIOFUEL_COMM, VIFM(i,"Blender",r) * qfm(i,"Blender",r) +
   7278                            VDFM(i,"Blender",r) * qfd(i,"Blender",r));
   7279   !
   7280    
   7281   ! Linking equation for tying different mandates together:
   7282       - to include imports in the scheme, swap bioimpslack with tpm for a given
   7283           biofuel/region
   7284       - to include secondary domestic biofuels, swap renewslack with tpd for a
   7285           second biofuel; if imports are also to be included, then add the above
   7286           swap as well. !
   7287   !
   7288   Variable (all,i,BIOFUEL_COMM)(all,r,REG)
   7289       bioimpslack(i,r)
   7290   # exogenize when wish to iclude imports in shock #;
   7291   Equation E_BIOIMPSLACK
   7292   (all,i,BIOFUEL_COMM)(all,r,REG)
   7293       tpm(i,r) = tpd(i,r) + bioimpslack(i,r);
   7294    
   7295    
   7296   Variable (all,i,BIOFUEL_COMM)(all,r,REG)
   7297       renewslack(i,r)
   7298   # exogenize when wish to link other renewables to national target #;
   7299   Equation E_RENEWSLACK
   7300   (all,i,BIOFUEL_COMM)(all,r,REG)
   7301       biooiluse(i,r) = biofueluse(r) + renewslack(i,r);
   7302   !
   7303    
   7304   !<
   7305       ---------------------------
   7306       G. YIELD TRACKING MODULE
   7307       ---------------------------
   7308    
   7309   - This module reads in initial yield as well as production,
   7310     in metric tonnes and updates these levels variables.
   7311   >!
   7312    
   7313   Variable (LEVELS) (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7314       PRODUCTION(i,j,r)
   7315       # millions of bushels or tonnes (use "PRBU" for tonnes only)#;
   7316   Read
   7317       PRODUCTION from file GTAPDATA header "PRDN";
   7318    
   7319   !< Unit "bushels" are used for CrGrains, OthGrains, and Soybeans.
   7320       For Sugarcane and OthAgri sectors are measured in "tonnes"  >!
   7321    
   7322   Subset
   7323       CROP_INDS is subset of NSAV_COMM;
Declaration of 'CROP_INDS' as a SUBSET of 'NSAV_COMM'.
%% Information only.
    This subset relation is a consequence of earlier subset declarations.
   7324    
   7325   Equation E_PRODUCTION
   7326   # eq'n computes change in crop production by AEZ #
   7327   (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7328       p_PRODUCTION(i,j,r) = p_HARVSTAREA_L(i,j,r)
   7329               - harvstslack(i,r) + [qo(j,r) - qoes(i,j,r)] ;
   7330    
   7331   Zerodivide (zero_by_zero) default 0;
   7332   Zerodivide (nonzero_by_zero) default 0;
   7333    
   7334   ! Compute yield from production and harvested area to ensure consistency !
   7335    
   7336   Coefficient (all,j,CROP_INDS)(all,r,REG)
   7337       YIELD(j,r)
   7338       # av. national yield in millions of bushels or tonnes #;
   7339   Variable (all,j,CROP_INDS)(all,r,REG)
   7340       p_YIELD(j,r)
   7341     # av. national yield in millions of bushels or tonnes #;
   7342   Formula (initial)
   7343   (all,j,CROP_INDS)(all,r,REG)
   7344       YIELD(j,r) =
   7345       sum(i,AEZ_COMM, PRODUCTION(i,j,r))/sum(i,AEZ_COMM, HARVSTAREA_L(i,j,r));
   7346   Update (all,j,CROP_INDS)(all,r,REG)
   7347       YIELD(j,r) = p_YIELD(j,r);
   7348    
   7349   Equation E_YIELD
   7350   # eq'n computes av. national yield in millions of bushels or tonnes #
   7351   (all,j,CROP_INDS)(all,r,REG)
   7352       p_YIELD(j,r) = qo(j,r) - harvstcom(j,r);
   7353    
   7354   Zerodivide (zero_by_zero) off;
   7355   Zerodivide (nonzero_by_zero) off;
   7356    
   7357   ! TH: Decomposing global yield changes. !
   7358    
   7359   Variable (LEVELS)(all,j,CROP_INDS)
   7360       YIELDGL(j)
   7361       # av. global yield in millions of bushels or tonnes #;
   7362   Formula & Equation (LEVELS) E_YIELDGL (all,j,CROP_INDS)
   7363       YIELDGL(j) = sum(r, REG, sum(i,AEZ_COMM, PRODUCTION(i,j,r)))
   7364                  / sum(r,REG,sum(i,AEZ_COMM,HARVSTAREA_L(i,j,r)));
       !  EQUATION(LINEAR) E_YIELDGL
          (ALL,j,CROP_INDS) YIELDGL(j) * p_YIELDGL(j) = 1.0 / SUM(r,REG,

          SUM(i,AEZ_COMM,HARVSTAREA_L(i,j,r))) * SUM(r,REG,SUM(i,AEZ_COMM,

          PRODUCTION(i,j,r) * p_PRODUCTION(i,j,r))) - SUM(r,REG,SUM(i,AEZ_COMM,

          PRODUCTION(i,j,r))) / {[SUM(r,REG,SUM(i,AEZ_COMM,HARVSTAREA_L(i,j,r))

          )]^[2  ]} * SUM(r,REG,SUM(i,AEZ_COMM,HARVSTAREA_L(i,j,r) * 

          p_HARVSTAREA_L(i,j,r))) ;  ! 

   7365    
   7366   Coefficient (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7367       PRODUCTSHR(i,j,r)
   7368       # share of production in AEZ i, region r of sector j's global output #;
   7369   Formula
   7370   (all,i,AEZ_COMM)(all,j,CROP_INDS)(all,r,REG)
   7371       PRODUCTSHR(i,j,r) =
   7372               PRODUCTION(i,j,r)/sum(k,REG, sum(m,AEZ_COMM, PRODUCTION(m,j,k)));
   7373    
   7374   Variable (all,j,CROP_INDS)
   7375       globalouput(j)
   7376       # sector j's global output chng #;
   7377   Equation E_GLOBALOUTPUT
   7378   # sector j's global output chng #
   7379   (all,j,CROP_INDS)
   7380       globalouput(j) = sum(r,REG, sum(i,AEZ_COMM, PRODUCTSHR(i,j,r) * qo(j,r)));
   7381    
   7382   Set EN_INDS # energy producing industries #
   7383   (coal, oil, gas, oil_pcts, blender, electricity, EthanolC, Ethanol2, Biod_soy,
   7384    Biod_palm, Biod_oth);
   7385    
   7386   Subset
   7387       EN_INDS is subset of ALL_INDS;
   7388    
   7389   Zerodivide (zero_by_zero) default 0;
   7390   Zerodivide (nonzero_by_zero) default 0;
   7391    
   7392   Coefficient(all,j,CROP_INDS)(all,r,REG)
   7393       CONSUMPTION(j,r)
   7394       #not for energy production of sector j's global output, by AEZ and region#;
   7395   Formula
   7396   (all,j,CROP_INDS)(all,r,REG)
   7397       CONSUMPTION(j,r) = sum(i, AEZ_COMM, PRODUCTION(i,j,r))
   7398               * (1 - sum(k,EN_INDS,VDFM(j,k,r))/VOM(j,r));
   7399    
   7400   Coefficient (all,j,CROP_INDS)(all,r,REG)
   7401       CONSSHR(j,r);
   7402   Formula
   7403   (all,j,CROP_INDS)(all,r,REG)
