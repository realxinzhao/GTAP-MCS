   2516   Equation CDDGCPRICE # price of CDDGC feed#
   2517   (all,j,ALL_INDS)(all,r,REG)
   2518       pf("CDDGC",j,r)
   2519           = sum(k,CDDGC_COMM, CSHCDDGC(k,j,r) * [pf(k,j,r) - af(k,j,r)]);
