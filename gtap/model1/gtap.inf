   3428   !SSA Yield Stuff!
   3429    
   3430    
   3431   ZERODIVIDE (ZERO_BY_ZERO) DEFAULT 0 ;
   3432   ZERODIVIDE DEFAULT 1 ;
   3433    
   3434   Coefficient (all,j,ALL_INDS)(all,r,reg)
   3435       TOTALC(j,r);
   3436   Formula (all,j,ALL_INDS)(all,r,reg)
   3437       TOTALC(j,r) =
   3438               sum(i, demd_comm,(VFA(i,j,r)));
   3439   Coefficient (all,j,ALL_INDS)(all,r,reg)
   3440       LANDCOST(j,r);
   3441   Formula (all,j,ALL_INDS)(all,r,reg)
   3442       LANDCOST(j,r) =
   3443               sum(a, AEZ_COMM,(VFA(a,j,r)));
   3444    
   3445   Coefficient(all,j,ALL_INDS)(all,r,reg)
   3446       THETAi(j,r) #Endowment's cost share in value-added#;
   3447   Formula(all,j,ALL_INDS)(all,r,reg)
   3448       THETAi(j,r) = LANDCOST(j,r)/TOTALC(j,r);
   3449    
   3450   Coefficient (parameter)(all,j,ALL_INDS)(all,r,REG)
   3451       YDONOFF(j,r) #Turn off the yield calibrated ESUBVA's by setting to zero#;
   3452   Read YDONOFF from file GTAPPARM header "YD01";
   3453    
   3454   Coefficient (parameter)(all,r,REG)
   3455       YDREGSCALE(r) #Scale up or down the target yield elast for a region#;
   3456   Read YDREGSCALE from file GTAPPARM header "YDRS";
   3457    
   3458   Coefficient (parameter) (all,j,ALL_INDS)(all,r,REG)
   3459       YDE_Target(j,r) #Scalar yield elasticity target, read from PARM for SSA#;
   3460   Read YDE_Target from file GTAPPARM header "YDEL";
   3461    
   3462    
   3463   Coefficient (parameter) (all,j,ALL_INDS)(all,r,REG)
   3464       ESUBVA1(j,r)
   3465       # elasticity of substitution in value-added-energy subproduction #;
   3466   Read
   3467       ESUBVA1 from file GTAPPARM header "ESBV";
   3468    
   3469    
   3470   ! CRP - nonzero default changed from 0 to 1 !
   3471   zerodivide (nonzero_by_zero) default 1;
   3472   zerodivide (zero_by_zero) default 0;
   3473    
   3474   Coefficient (parameter) (all,j,ALL_INDS)(all,r,REG)
   3475       ESUBVA(j,r)
   3476       # elasticity of substitution in value-added-energy subproduction #;
   3477   Formula(initial) (all,j,ALL_INDS)(all,r,REG)
   3478       ESUBVA(j,r) = if(THETAi(j,r)*YDONOFF(j,r) le 0, ESUBVA1(j,r))
   3479                   + if(THETAi(j,r)*YDONOFF(j,r) gt 0,
   3480                        YDREGSCALE(r)*YDE_Target(j,r)/
   3481                                           [(1/THETAi(j,r))-1]);
   3482    
   3483   zerodivide (zero_by_zero) off;
   3484   zerodivide (nonzero_by_zero) off;
   3485    
   3486    
   3487   !SSA Yield Stuff!
