   1792   !
   1793      2-3.2 Biofuels Nest
   1794       ------------------!
   1795    
   1796   !Xin added electricity0 nest here!
   1797    
   1798   Formula (all,r,REG)
   1799       VPAS("electricity0",r) = sum(i,ELEC_COMM, VPAD(i,r));
   1800   Formula (all,r,REG)
   1801       VPA("electricity0",r) = VPAS("electricity0",r);
   1802    
   1803   Coefficient
   1804       SIZE_HELEC # size of ELEC_COMM set #;
   1805   Formula
   1806       SIZE_HELEC = sum(i,ELEC_COMM, 1);
   1807    
   1808   Coefficient (all,i,ELEC_COMM)(all,r,REG)
   1809       CSHHELEC(i,r)
   1810       # share of i in cost to j of elec subproduct #;
   1811   Formula (all,i,ELEC_COMM)(all,r,REG: VPAS("electricity0",r) = 0)
   1812       CSHHELEC(i,r) = 1.0 / SIZE_HELEC;
   1813   Formula (all,i,ELEC_COMM)(all,r,REG: VPAS("electricity0",r) > 0)
   1814       CSHHELEC(i,r) = VPA(i,r) / VPAS("electricity0",r);
   1815    
   1816   Equation HELECPRICE # price of henergy #
   1817   (all,r,REG)
   1818       pp("electricity0",r)= sum(k,ELEC_COMM, CSHHELEC(k,r)* pp(k,r));
   1819    
   1820   Coefficient (parameter) (all,r,REG)
   1821       ELELEC(r)
   1822       # elasticity of substitution in biooil subconsumption #;
   1823   Read
   1824       ELELEC from file GTAPPARM header "ELEC";
   1825    
   1826   Equation HELECDEMAND
   1827   # demand for inputs into biooil subconsumption #
   1828   (all,i,ELEC_COMM)(all,r,REG)
   1829       qp(i,r) = qp("electricity0",r)
   1830           - ELELEC(r) * [pp(i,r)- pp("electricity0",r)];
   1831    
