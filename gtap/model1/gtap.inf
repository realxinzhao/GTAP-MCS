   2280    
   2281    
   2282   Zerodivide (zero_by_zero) default 0;
   2283   Coefficient (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,s,REG)
   2284       FMSHR(i,j,s) # share of firms' imports in dom. composite, agent's prices #;
   2285   Formula (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,s,REG)
   2286       FMSHR(i,j,s) = VIFA(i,j,s) / VFAD(i,j,s);
   2287   Equation ICOMPRICE
   2288   # industry price for composite commodities (HT 30) #
   2289   (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   2290       pft(i,j,r) = FMSHR(i,j,r) * pfm(i,j,r) + [1 - FMSHR(i,j,r)] * pfd(i,j,r);
   2291   Zerodivide (zero_by_zero) off;
   2292    
   2293   Equation INDIMP
   2294   # industry j demands for composite import i (HT 31) #
   2295   (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,s,REG)
   2296       qfm(i,j,s) = qft(i,j,s) - ESUBD(i) * [pfm(i,j,s) - pft(i,j,s)];
   2297    
   2298   Equation INDDOM
   2299   # industry j demands for domestic good i (HT 32) #
   2300   (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,s,REG)
   2301       qfd(i,j,s) = qft(i,j,s) - ESUBD(i) * [pfd(i,j,s) - pft(i,j,s)];
