   1182   !<
   1183       Regional Expenditure and Income
   1184       -------------------------------
   1185    
   1186       Regional income is allocated between private consumption expenditure,
   1187       government consumption expenditure, and saving.
   1188    
   1189    
   1190       Expenditure of Regional Household
   1191       .................................
   1192    
   1193                  Regional household(h.h.)
   1194                   Expenditure = Income
   1195                            _/|\_
   1196                          _/  |  \_
   1197                        _/    |    \_
   1198                      _/      |      \_
   1199                    _/        |        \_
   1200          Private h.h.        |        Government h.h.
   1201          ( PRIVEXP )         |          ( GOVEXP )
   1202             _/\_             |              _/\_
   1203           _/    \_           |            _/    \_
   1204          /        \          |           /        \
   1205       Domestic  Imported   Savings    Domestic    Imported
   1206        Goods     Goods     ( SAVE )    Goods       Goods
   1207       ( VDPA )  ( VIPA )              ( VDGA )    ( VIGA )
   1208    
   1209       Note: The coefficients at the ends of branches are
   1210             Base Data, e.g., VDPA, SAVE.
   1211   >!
   1212    
   1213   ! government consumption expenditure, GOVEXP !
   1214   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1215       VGA(i,r) # government consn expenditure on i in r at agent's prices #;
   1216   Formula (all,i,TRAD_COMM)(all,s,REG)
   1217       VGA(i,s) = VDGA(i,s) + VIGA(i,s);
   1218   Coefficient (all,r,REG)
   1219       GOVEXP(r) # government expenditure in region r #;
   1220   Formula (all,r,REG)
   1221       GOVEXP(r) = sum(i,TRAD_COMM, VGA(i,r));
   1222    
   1223   ! ECONS: Change VPA to VPAD !
   1224    
   1225   ! private consumption expenditure, PRIVEXP !
   1226   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1227       VPAD(i,r) # private hhld expenditure on i in r valued at agent's prices #;
   1228   Formula (all,i,TRAD_COMM)(all,s,REG)
   1229       VPAD(i,s) = VDPA(i,s) + VIPA(i,s);
   1230   Coefficient (all,r,REG)
   1231       PRIVEXP(r) # private consumption expenditure in region r #;
   1232   Formula (all,r,REG)
   1233       PRIVEXP(r) = sum(i,TRAD_COMM, VPAD(i,r));
   1234    
   1235   ! aggregate expenditure + saving = aggregate income !
   1236   Coefficient (all,r,REG)
   1237       INCOME(r) # level of expenditure, which equals NET income in region r #;
   1238   Formula (all,r,REG)
   1239       INCOME(r) = PRIVEXP(r) + GOVEXP(r) + SAVE(r);
   1240    
   1241    
   1242   !<
   1243       Indirect Tax Receipts
   1244       ---------------------
   1245   >!
   1246    
   1247   Coefficient (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   1248       DFTAX(i,j,r) # ordinary tax on intermediate usage of domestic product #;
   1249   Formula (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   1250       DFTAX(i,j,r) = VDFA(i,j,r) - VDFM(i,j,r);
   1251    
   1252   Coefficient (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   1253       IFTAX(i,j,r) # ordinary tax on intermediate usage of imports #;
   1254   Formula (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   1255       IFTAX(i,j,r) = VIFA(i,j,r) - VIFM(i,j,r);
   1256    
   1257   !Farzad: Buifules 2015: separate fro Boioil and non biooil!
   1258   Coefficient (all,r,REG)
   1259    TIUNBIO(r) # ordinary tax on intermediate usage for non biooil products#;
   1260   Formula (all,r,REG)
   1261    TIUNBIO(r) = sum(j,ALL_INDS, sum(i,NBIOOIL_COMM, DFTAX(i,j,r) + IFTAX(i,j,r)));
   1262    
   1263   Coefficient (all,r,REG)
   1264    TIUBIO(r) # ordinary tax on intermediate usage for biooil products#;
   1265   Formula (all,r,REG)
   1266    TIUBIO(r) = sum(j,ALL_INDS, sum(i,BIOOIL_COMM, DFTAX(i,j,r) + IFTAX(i,j,r)));
   1267    
   1268   Coefficient (all,r,REG)
   1269       TIU(r) # ordinary tax on intermediate usage #;
   1270    
   1271   Formula (all,r,REG)
   1272     TIU(r) = TIUNBIO(r) + TIUBIO(r);
   1273    
   1274   !Formula (all,r,REG)
   1275       TIU(r) = sum(j,ALL_INDS, sum(i,TRAD_COMM, DFTAX(i,j,r) + IFTAX(i,j,r)));!
