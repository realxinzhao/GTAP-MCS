   2329   !<
   2330       3-4. Input-Augmenting Technological Change
   2331       ------------------------------------------
   2332   >!
   2333    
   2334   !Farzad 2015: The following tech change is introduced to adjust the energy
   2335    content of ethanol!
   2336   Variable (all,i,FIRM_COMM)(all,r,REG)
   2337       afenergy(i,r) # factor i augmenting tech change, worldwide #;
   2338   Variable (all,i,FIRM_COMM)
   2339       afcom(i) # factor i augmenting tech change, worldwide #;
   2340   Variable (all,j,ALL_INDS)
   2341       afsec(j) # factor augmenting tech change in sector j, worldwide #;
   2342   Variable (all,r,REG)
   2343       afreg(r) # factor augmenting tech change in region r #;
   2344   Variable (all,i,FIRM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2345       afall(i,j,r) # input i augmenting tech change in sector j of r #;
   2346    
   2347   !SOYF: Defining techchange for cropland pasture. To use this off yield for
   2348   cropland paasture!
   2349    
   2350   Set AEZ_COMM
   2351      Read elements from file GTAPSETS header "AEZ";
   2352   Subset
   2353       AEZ_COMM is subset of ENDW_COMM;
   2354    
   2355   Set NAEZ=FIRM_COMM-AEZ_COMM;
   2356    
   2357   Set PASTCR (Pasturecrop);
   2358    
   2359   Subset
   2360       PASTCR is subset of ALL_INDS;
   2361    
   2362   Set NONPASTCR =ALL_INDS-PASTCR;
   2363    
   2364   Variable (all,i,FIRM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2365       afpasture(i,j,r) # pasture tech change of r by aez#;
   2366    
   2367   Coefficient (parameter)(all,r,REG)
   2368       PASTUREEL(r)  # pasture tech change of r by aez#;
   2369   Read PASTUREEL from file GTAPPARM header "PAEL";
   2370    
   2371    
   2372   !<<Begin Add codes to further adjust cropland pasture yield !
   2373    
   2374   Coefficient (parameter)
   2375       PAELADJUST  # pasture tech change adjustment factor#;
   2376   Read PAELADJUST from file GTAPPARM header "PAAD";
   2377    
   2378    
   2379   Coefficient (parameter)
   2380       (all,i,FIRM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2381       PASTUREON(i,j,r) # pasture tech ON_OFF#;
   2382       !In the following read in PAOF for off case and read in PAON for on case!
   2383   Read PASTUREON from file GTAPPARM header "PAON";
   2384    
   2385   Variable (all,i,FIRM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2386      upadvmiscshare(i,j,r) # percentage change in share of advf_Misc#;
   2387    
   2388   Coefficient (all,i,FIRM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2389       ADVMISCSHARE(i,j,r) # Advf_Fuel share in eth+advf_Mics#;
   2390    
   2391   Update (all,i,FIRM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2392       ADVMISCSHARE(i,j,r) = upadvmiscshare(i,j,r) ;
   2393    
   2394   Read
   2395       ADVMISCSHARE from file GTAPDATA header "ADFS";
   2396    
   2397   Zerodivide (zero_by_zero) default 0;
   2398   zerodivide (nonzero_by_zero) default 0;
   2399    
   2400   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   2401   MISCSHA0(i,r)
   2402   #sahre of misc in value added of misc plus swit#;
   2403    
   2404   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   2405   MISCSHA(i,r)
   2406   #sahre of misc in value added of misc plus swit plus cropland pasture #;
   2407    
   2408   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   2409   SWITSHA0(i,r)
   2410   #sahre of swit in value added of misc plus swit#;
   2411    
   2412   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   2413   SWITSHA(i,r)
   2414   #sahre of swit in value added of misc plus swit plus cropland pasture #;
   2415    
   2416    
   2417   Coefficient (all,i,AEZ_COMM)(all,r,REG)
   2418   CROPPASSHA(i,r)
   2419   #sahre of crop_pasture in value added of misc plus swit plus crop-pasture#;
   2420    
   2421   Formula
   2422   (all,i,AEZ_COMM)(all,r,REG)
   2423   MISCSHA0(i,r)=EVFA(i,"Miscanthus",r)/
   2424          (EVFA(i,"Miscanthus",r)+EVFA(i,"Switchgrass",r));
   2425    
   2426   Formula
   2427   (all,i,AEZ_COMM)(all,r,REG)
   2428   MISCSHA(i,r)=EVFA(i,"Miscanthus",r)/
   2429          (EVFA(i,"Miscanthus",r)+EVFA(i,"Switchgrass",r)+EVFA(i,"Pasturecrop",r));
   2430   Formula
   2431   (all,i,AEZ_COMM)(all,r,REG)
   2432   SWITSHA0(i,r)=EVFA(i,"Switchgrass",r)/
   2433          (EVFA(i,"Miscanthus",r)+EVFA(i,"Switchgrass",r));
   2434    
   2435   Formula
   2436   (all,i,AEZ_COMM)(all,r,REG)
   2437   SWITSHA(i,r)=EVFA(i,"Switchgrass",r)/
   2438          (EVFA(i,"Miscanthus",r)+EVFA(i,"Switchgrass",r)+EVFA(i,"Pasturecrop",r));
   2439    
   2440   Formula
   2441   (all,i,AEZ_COMM)(all,r,REG)
   2442   CROPPASSHA(i,r)=EVFA(i,"Pasturecrop",r)/
   2443          (EVFA(i,"Miscanthus",r)+EVFA(i,"Switchgrass",r)+EVFA(i,"Pasturecrop",r));
   2444   Zerodivide (zero_by_zero) off;
   2445   zerodivide (nonzero_by_zero) off;
   2446    
   2447   Equation E_advfmiscshare1 # update equation for adfv_misc shares  #
   2448       (all,i,AEZ_COMM)(all,r,REG)
   2449        upadvmiscshare(i,"Pasturecrop",r) =
   2450   +[MISCSHA0(i,r)*qf(i,"Miscanthus",r)+SWITSHA0(i,r)*qf(i,"Switchgrass",r)]
   2451   -[MISCSHA(i,r)*qf(i,"Miscanthus",r)+SWITSHA(i,r)*qf(i,"Switchgrass",r)+
   2452   CROPPASSHA(i,r)*qf(i,"Pasturecrop",r)];
   2453    
   2454   Equation E_advfmiscshare2 # update equation for adfv_misc shares  #
   2455       (all,i,NAEZ)(all,j,ALL_INDS)(all,r,REG)
   2456        upadvmiscshare(i,j,r) = 0;
   2457    
   2458   Equation E_advfmiscshare3 # update equation for adfv_misc shares  #
   2459       (all,i,AEZ_COMM)(all,j,NONPASTCR)(all,r,REG)
   2460        upadvmiscshare(i,j,r) = 0;
   2461   !<<end!
   2462    
   2463    
   2464   !<<modify the first equation!
   2465   Equation PASTUREAF1
   2466   # Determine tech change in cropland paasture sector #
   2467   (all,i,AEZ_COMM)(all,r,REG)
   2468   afpasture(i,"Pasturecrop",r)=PASTUREEL(r)*
   2469                    (1+PAELADJUST*ADVMISCSHARE(i,"Pasturecrop",r))
   2470                    * PASTUREON(i,"Pasturecrop",r)*pf(i,"Pasturecrop",r);
   2471    
   2472   Equation PASTUREAF2
   2473   # Determine tech in NAEZ in all sectors #
   2474   (all,i,NAEZ)(all,j,ALL_INDS)(all,r,REG)
   2475   afpasture(i,j,r)=0;
   2476    
   2477   Equation PASTUREAF3
   2478   # Determine tech change in AEZ for non cropland paasture sectors #
   2479   (all,i,AEZ_COMM)(all,j,NONPASTCR)(all,r,REG)
   2480   afpasture(i,j,r)=0;
   2481    
   2482    
   2483    
   2484   Equation AFWORLD
   2485   # sector/region specific rate of factor i augmenting tech change #
   2486   (all,i,FIRM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2487       af(i,j,r) = afcom(i)+afsec(j)+afreg(r)+afall(i,j,r)+afpasture(i,j,r)+
   2488       afenergy(i,r);
