   1558   !<
   1559       2-0. Module-Specific Variables
   1560       ------------------------------
   1561    
   1562       only used in this Private Consumption module
   1563   >!
   1564    
   1565   Set
   1566       SUBCDE_COMM # subconsumption aggregates entering the CDE # (henergy, hvol);
   1567   Set
   1568       SUBLOW_COMM # lower level subconsumption aggregates # (hbiooil,electricity0);
   1569   Set
   1570       SUBC_COMM # subconsumption aggregates # = SUBCDE_COMM union SUBLOW_COMM;
   1571   Set
   1572       HHLD_COMM # commodities demanded by hhlds # = TRAD_COMM union SUBC_COMM;
   1573   Set
   1574       NEGY_COMM # non-energy commodities  # = TRAD_COMM - EGY_COMM;
   1575   Set
   1576       CDE_COMM # top level private consumption #
   1577       maximum size 10 read elements from file GTAPSETS header "CDE";
   1578   !<< The "CDE" in GTAPSETS is revised accordingly"!
   1579    
   1580   Subset CDE_COMM is subset of HHLD_COMM ;
   1581    
   1582   Coefficient (all,i,HHLD_COMM)(all,r,REG)
   1583       VPA(i,r) # consumer expend. on i by j in r valued at agent's prices #;
   1584   Formula (all,i,TRAD_COMM)(all,r,REG)
   1585       VPA(i,r) = VPAD(i,r);
   1586    
   1587   ! Note that VPA(henergy,r) will be assigned below. !
   1588    
   1589   Coefficient (all,i,SUBC_COMM)(all,r,REG)
   1590       VPAS(i,r) # hhld expend. on i in r valued at agent's prices #;
   1591    
   1592   Variable (all,i,TRAD_COMM)(all,r,REG)
   1593       ppt(i,r) # private consumption price for commodity i in region r #;
   1594   Variable (all,i,TRAD_COMM)(all,r,REG)
   1595       qpt(i,r) # private hhld demand for commodity i in region r #;
   1596    
   1597   ! The following price and quantity variables are used in the CDE module !
   1598    
   1599   Variable (all,i,HHLD_COMM)(all,r,REG)
   1600       pp(i,r) # private consumption price for commodity i in region r #;
   1601   Variable (all,i,HHLD_COMM)(all,r,REG)
   1602       qp(i,r) # private hhld demand for commodity i in region r #;
   1603    
   1604   !<
   1605       2-1. Composite Tradeables
   1606       -------------------------
   1607   >!
   1608    
   1609   Variable (all,r,REG)
   1610       tp(r) # comm.-, source-gen. shift in tax on private cons. #;
   1611   !<
   1612       The variable tp(r) can be swapped with del_ttaxr(r) in order to generate a
   1613       tax replacement scenario, whereby taxes remain a constant share of
   1614       national income.
   1615   >!
   1616   Variable (all,i,TRAD_COMM)(all,r,REG)
   1617       consslack(i,r) # variable introduced to fix consumption at the base level #;
   1618    
   1619   Variable (all,i,TRAD_COMM)(all,r,REG)
   1620       tpd(i,r) # comm.-, source-spec. shift in tax on private cons. of dom. #;
   1621    
   1622   ! BIOFUELS: need to introduce an offsetting tax for renewable fuel mandates !
   1623    
   1624   Variable (all,r,REG)
   1625       tpbio(r) # comm.-, source-gen. tax to implement RFSS #;
   1626   Equation TPDSHIFTNBIO
   1627   # permits uniform consumption tax change #
   1628   (all,i,NBIOOIL_COMM)(all,r,REG)
   1629       atpd(i,r) = tpd(i,r) + tp(r) + consslack(i,r);
   1630    
   1631   Equation TPDSHIFTBIO
   1632   # permits uniform consumption tax change #
   1633   (all,i,BIOOIL_COMM)(all,r,REG)
   1634       atpd(i,r) = tpd(i,r) + tp(r) + tpbio(r) + consslack(i,r);
   1635    
   1636   ! And do the same for tpm below. !
   1637    
   1638    
   1639   Equation PHHDPRICE
   1640   # taxation of private consumption of domestic product #
   1641   (all,i,TRAD_COMM)(all,r,REG)
   1642       ppd(i,r) = pm(i,r) + atpd(i,r);
   1643    
   1644   ! BIOFUELS: need to introduce an offsetting tax for renewable fuel mandates !
   1645    
   1646   Variable (all,i,TRAD_COMM)(all,r,REG)
   1647       tpm(i,r) # comm.-, source-spec. shift in tax on private cons. of imp. #;
   1648   Equation TPMSHIFTNBIO
   1649   # permits uniform consumption tax change #
   1650   (all,i,NBIOOIL_COMM)(all,r,REG)
   1651       atpm(i,r) = tpm(i,r) + tp(r) + consslack(i,r);
   1652    
   1653   Equation TPMSHIFTBIO
   1654   # permits uniform consumption tax change #
   1655   (all,i,BIOOIL_COMM)(all,r,REG)
   1656       atpm(i,r) = tpm(i,r) + tp(r) + tpbio(r) + consslack(i,r);
   1657    
   1658   ! BIOFUELS: end of modifications !
   1659    
   1660    
   1661   Equation PHHIPRICES
   1662   # taxation of private consumption of imports #
   1663   (all,i,TRAD_COMM)(all,r,REG)
   1664       ppm(i,r) = pim(i,r) + atpm(i,r);
   1665    
   1666   ! BIOFUELS: Separate out the biofuels component of this to permit
   1667       implementation of a RFS !
   1668    
   1669   Variable (change) (all,r,REG)
   1670       del_taxrpcnbio(r) # change in ratio cons tax on nonbiooil to INCOME #;
   1671   Variable (change) (all,r,REG)
   1672       del_taxrpcbio(r) # change in ratio cons tax on biooil to INCOME #;
   1673    
   1674   Equation TPCRATIONBIO
   1675   # change in ratio of private consumption tax payments to regional income #
   1676   (all,r,REG)
   1677       100.0 * INCOME(r) * del_taxrpcnbio(r) + TPCNBIO(r) * y(r)
   1678           = sum(i,NBIOOIL_COMM,
   1679               VDPA(i,r) * atpd(i,r) + DPTAX(i,r) * [pm(i,r) + qpd(i,r)])
   1680           + sum(i,NBIOOIL_COMM,
   1681               VIPA(i,r) * atpm(i,r) + IPTAX(i,r) * [pim(i,r) + qpm(i,r)]);
   1682    
   1683   Equation TPCRATIOBIO
   1684   # change in ratio of private consumption tax payments to regional income #
   1685   (all,r,REG)
   1686       100.0 * INCOME(r) * del_taxrpcbio(r) + TPCBIO(r) * y(r)
   1687           = sum(i,BIOOIL_COMM,
   1688               VDPA(i,r) * atpd(i,r) + DPTAX(i,r) * [pm(i,r) + qpd(i,r)])
   1689           + sum(i,BIOOIL_COMM,
   1690               VIPA(i,r) * atpm(i,r) + IPTAX(i,r) * [pim(i,r) + qpm(i,r)]);
   1691    
   1692   Equation TPCRATIO
   1693   # change in ratio of private consumption tax payments to regional income #
   1694   (all,r,REG)
   1695       del_taxrpc(r)
   1696           = del_taxrpcnbio(r) + del_taxrpcbio(r);
   1697    
   1698   ! BIOFUELS end !
   1699    
   1700   Zerodivide (zero_by_zero) default 0;
   1701   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   1702       PMSHR(i,s) # share of imports for priv hhld at agent's prices #;
   1703   Formula (all,i,TRAD_COMM)(all,s,REG)
   1704       PMSHR(i,s) = VIPA(i,s) / VPA(i,s);
   1705   Zerodivide (zero_by_zero) off;
   1706    
   1707   Equation PCOMPRICE
   1708   # private consumption price for composite commodities (HT 47) #
   1709   (all,i,TRAD_COMM)(all,s,REG)
   1710       ppt(i,s) = PMSHR(i,s) * ppm(i,s) + [1 - PMSHR(i,s)] * ppd(i,s);
   1711    
   1712   Equation PHHLDDOM
   1713   # private consumption demand for domestic goods (HT 48) #
   1714   (all,i,TRAD_COMM)(all,s,REG)
   1715       qpd(i,s) = qpt(i,s) + ESUBD(i) * [ppt(i,s) - ppd(i,s)];
   1716    
   1717   Equation PHHLDAGRIMP
   1718   # private consumption demand for aggregate imports (HT 49) #
   1719   (all,i,TRAD_COMM)(all,s,REG)
   1720       qpm(i,s) = qpt(i,s) + ESUBD(i) * [ppt(i,s) - ppm(i,s)];
   1721    
   1722    
   1723   !<
   1724       2-2. Notational Links
   1725       ---------------------
   1726   >!
   1727    
   1728   Formula (all,i,TRAD_COMM)(all,r,REG)
   1729       VPA(i,r) = VPAD(i,r);
   1730    
   1731   ! Equations linking qph with qp and pph with pp for non-energy commodities
   1732       follow. The energy commodities are demanded at the qp level. !
   1733    
   1734   Equation E_QPT
   1735   # linking equation for quantities #
   1736   (all,i,TRAD_COMM)(all,r,REG)
   1737       qpt(i,r) = qp(i,r) ;
   1738    
   1739   Equation E_PPT
   1740   # linking variable for prices #
   1741   (all,i,TRAD_COMM)(all,r,REG)
   1742       ppt(i,r) = pp(i,r) ;
   1743    
   1744   !<
   1745       2-3.1 Biofuels Nest
   1746       ------------------
   1747   >!
   1748    
   1749   !Farzad 2015: First add a variables to account for energy content of ethanol!
   1750   !<<The nest for HHD was not changed, since the blender was added!
   1751    Variable (all,i,BIOOIL_COMM)(all,r,REG)
   1752       ahenergy(i,r) # factor to adjust energy content of biofuels in region r#;
   1753    
   1754   Formula (all,r,REG)
   1755       VPAS("hbiooil",r) = sum(i,BIOOIL_COMM, VPAD(i,r));
   1756   Formula (all,r,REG)
   1757       VPA("hbiooil",r) = VPAS("hbiooil",r);
   1758    
   1759   Coefficient
   1760       SIZE_HBIOIL # size of BIOOIL_COMM set #;
   1761   Formula
   1762       SIZE_HBIOIL = sum(i,BIOOIL_COMM, 1);
   1763    
   1764   Coefficient (all,i,BIOOIL_COMM)(all,r,REG)
   1765       CSHHBIOIL(i,r)
   1766       # share of i in cost to j of henergy subproduct #;
   1767   Formula (all,i,BIOOIL_COMM)(all,r,REG: VPAS("hbiooil",r) = 0)
   1768       CSHHBIOIL(i,r) = 1.0 / SIZE_HBIOIL;
   1769   Formula (all,i,BIOOIL_COMM)(all,r,REG: VPAS("hbiooil",r) > 0)
   1770       CSHHBIOIL(i,r) = VPA(i,r) / VPAS("hbiooil",r);
   1771    
   1772   Equation HBIOILPRICE # price of henergy #
   1773   (all,r,REG)
   1774       pp("hbiooil",r)
   1775           = sum(k,BIOOIL_COMM, CSHHBIOIL(k,r)* [pp(k,r)-ahenergy(k,r)]);
   1776    
   1777   Coefficient (parameter) (all,r,REG)
   1778       ELHBIOIL(r)
   1779       # elasticity of substitution in biooil subconsumption #;
   1780   Read
   1781       ELHBIOIL from file GTAPPARM header "ELHB";
   1782    
   1783   Equation HBIOILDEMAND
   1784   # demand for inputs into biooil subconsumption #
   1785   (all,i,BIOOIL_COMM)(all,r,REG)
   1786       qp(i,r) = -ahenergy(i,r)+qp("hbiooil",r)
   1787           - ELHBIOIL(r) * [pp(i,r)-ahenergy(i,r) - pp("hbiooil",r)];
   1788    
   1789    
