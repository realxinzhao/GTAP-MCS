   3515   !Farzad added in 2015 to take care of energy content of blend!
   3516   !<<added new biofuels into blender!
   3517   Set
   3518       BLEND_COMM # Inputs into blend #
   3519       (Oil_Pcts, ethanol1, ethanol2, ethanol3, biod_Soy, Biod_Palm, Biod_Rape,
   3520        Biod_Oth,GRNATJ_J,GRNETJ_J,SUGATJ_J,SUGETJ_J,SUGSIP_J,SOYHRJ,RAPHRJ,PLMHRJ,CAMHRJ_J,
   3521       CRNHRJ_J,MSCFTJ,SWGFTJ,POPFTJ,MSCATJ,SWGATJ);
   3522    
   3523   Subset
   3524       BLEND_COMM is subset of TRAD_COMM;
   3525    
   3526   VARIABLE (LEVELS) (all,i,BLEND_COMM)(all,r,REG)
   3527       BIOOIL0(i,r)
   3528       # volume of biofuels usage in Quad Btu LHV #;
   3529   Read
   3530       BIOOIL0 from file GTAPDATA header "BOI0";
   3531   !
   3532   COEFFICIENT  (all,i,BLEND_COMM)(all,r,REG)
   3533   TOTUSE0(i,r)  # total use of fuels in blend #;
   3534   FORMULA
   3535   (all,i,BLEND_COMM)(all,r,REG)
   3536       TOTUSE0(i,r) =
   3537       VIFM(i,"Blender",r) + VDFM(i,"Blender",r);
   3538   Equation E_BIOUSE0
   3539   (all,i,BLEND_COMM)(all,r,REG)
   3540       TOTUSE0(i,r) * biooiluse0(i,r) = VIFM(i,"Blender",r) * qfm(i,"Blender",r)+
   3541                                        VDFM(i,"Blender",r) * qfd(i,"Blender",r);
   3542   !
   3543   Zerodivide (zero_by_zero) default 0;
   3544   coefficient(all,i,BLEND_COMM)(all,r,REG)
   3545   TOTUSE0SHR1(i,r);
   3546   coefficient(all,i,BLEND_COMM)(all,r,REG)
   3547   TOTUSE0SHR2(i,r);
   3548   FORMULA (all,i,BLEND_COMM)(all,r,REG)
   3549   TOTUSE0SHR1(i,r)=VIFM(i,"Blender",r)/[VIFM(i,"Blender",r) + VDFM(i,"Blender",r)];
   3550   FORMULA (all,i,BLEND_COMM)(all,r,REG)
   3551   TOTUSE0SHR2(i,r)=VDFM(i,"Blender",r)/[VIFM(i,"Blender",r) + VDFM(i,"Blender",r)];
   3552   Zerodivide (zero_by_zero) off;
   3553   Variable (all,i,BLEND_COMM)(all,r,REG)
   3554       biooiluse0(i,r)
   3555       # change in volume of biooils usage #;
   3556   Equation E_BIOUSE0
   3557   (all,i,BLEND_COMM)(all,r,REG)
   3558        biooiluse0(i,r) = TOTUSE0SHR1(i,r) * qfm(i,"Blender",r)+
   3559                                        TOTUSE0SHR2(i,r) * qfd(i,"Blender",r);
   3560   Equation E_BIOOIL0
   3561   (all,i,BLEND_COMM)(all,r,REG)
   3562       p_BIOOIL0(i,r) = biooiluse0(i,r);
   3563    
   3564   Coefficient (parameter) (all,i,BLEND_COMM)
   3565   GASOLINEQ (i) # 1/0.67 for ethanol and 1 for the rest read from PARM file#;
   3566   Read GASOLINEQ from file GTAPPARM header "GASO";
   3567    
   3568   Coefficient (parameter)
   3569       ECONOFF #Turn on & off energy content impact: 1=on 0=off#;
