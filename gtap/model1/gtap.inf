   4623   !<
   4624       8-1. Supply of Endowments by the Regional Household
   4625       ---------------------------------------------------
   4626   >!
   4627   !Farzad 2015: BIOFUELS: an offsetting tax on endowments incomes is included
   4628   to take care of RFS using tax on endowments and subsidy on biofules!
   4629    
   4630   Variable (all,r,REG)
   4631       towbio(r) # tax on endowments to implement RFSS #;
   4632    
   4633   !Farzad: The following should be exogenous and towbio endowgenous with RFS
   4634   implimentation using tax on endowmwnts!
   4635   Variable (all,r,REG)
   4636       del_taxrbioendw(r) # ratio of incoome tax plus tax credit on biofules) #;
   4637    
   4638   Equation FACTORINCPRICES
   4639   # eq'n links pre- and post-tax endowment supply prices (HT 15) #
   4640   (all,i,ENDW_COMM)(all,r,REG)
   4641       ps(i,r) = to(i,r) + towbio(r) + pm(i,r);
   4642    
   4643   Coefficient (all,r,REG)
   4644       TINC(r) # income tax payments in r #;
   4645   Formula (all,r,REG)
   4646       TINC(r) = sum(i,ENDW_COMM, PTAX(i,r));
   4647    
   4648   Equation TINCRATIO
   4649   # change in ratio of income tax payments to regional income #
   4650   (all,r,REG)
   4651       100.0 * INCOME(r) * del_taxrinc(r) + TINC(r) * y(r)
   4652           = sum(i,ENDW_COMM,
   4653               VOA(i,r) * [-to(i,r)-towbio(r)] + PTAX(i,r) * [pm(i,r) + qo(i,r)]);
   4654    
   4655   Equation REVNUETRAL
   4656   #Revenue neutral for RFS with tax ceredit on biofeuls and tax on endowments#
   4657   (all,r,REG)
   4658   del_taxrbioendw(r)= del_taxrinc(r)+del_taxroutbio(r);
   4659    
   4660   ! AEZ - create default share for REVSHR since some AEZs not present in region !
   4661    
   4662   Coefficient (all,i,ENDW_COMM)(all,j,ALL_INDS)(all,r,REG)
   4663       REVSHR(i,j,r);
   4664   Formula (all,i,ENDW_COMM)(all,j,ALL_INDS)(all,r,REG: VOM(i,r) = 0)
   4665       REVSHR(i,j,r) = 1.0 / SIZE_PROD;
   4666   Formula (all,i,ENDW_COMM)(all,j,ALL_INDS)(all,r,REG: VOM(i,r) > 0)
   4667       REVSHR(i,j,r) = VFM(i,j,r) / sum(k,ALL_INDS, VFM(i,k,r));
   4668    
   4669   Equation ENDW_PRICE
   4670   # eq'n generates the composite price for sluggish endowments (HT 50) #
   4671   (all,i,ENDWS_COMM)(all,r,REG)
   4672       pm(i,r) = sum(k,ALL_INDS, REVSHR(i,k,r) * pmes(i,k,r));
