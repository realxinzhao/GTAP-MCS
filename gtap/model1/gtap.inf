   1454   !<
   1455       1-0. Module-Specific Variables
   1456       ------------------------------
   1457    
   1458       only used in this Government Consumption module
   1459   >!
   1460    
   1461   Variable (all,i,TRAD_COMM)(all,r,REG)
   1462       pg(i,r) # government consumption price for commodity i in region r #;
   1463   Variable (all,i,TRAD_COMM)(all,r,REG)
   1464       qg(i,r) # government hhld demand for commodity i in region r #;
   1465    
   1466    
   1467   !<
   1468       1-1. Demands for Composite Goods
   1469       --------------------------------
   1470   >!
   1471    
   1472   Equation GPRICEINDEX
   1473   # definition of price index for aggregate gov't purchases (HT 40) #
   1474   (all,r,REG)
   1475       pgov(r) = sum(i,TRAD_COMM, [VGA(i,r) / GOVEXP(r)] * pg(i,r));
   1476    
   1477   Equation GOVDMNDS
   1478   # government consumption demands for composite commodities (HT 41) #
   1479   (all,i,TRAD_COMM)(all,r,REG)
   1480       qg(i,r) - pop(r) = ug(r) - [pg(i,r) - pgov(r)];
   1481    
   1482   Equation GOVU
   1483   # utility from government consumption in r #
   1484   (all,r,REG)
   1485       yg(r) - pop(r) = pgov(r) + ug(r);
   1486    
   1487    
   1488   !<
   1489       1-2. Composite Tradeables
   1490       -------------------------
   1491   >!
   1492    
   1493    
   1494   Equation GHHDPRICE
   1495   # taxation of government consumption of domestic product #
   1496   (all,i,TRAD_COMM)(all,r,REG)
   1497       pgd(i,r)= pm(i,r) + tgd(i,r);
   1498    
   1499    
   1500   Equation GHHIPRICES
   1501   # taxation of government consumption of imports #
   1502   (all,i,TRAD_COMM)(all,r,REG)
   1503       pgm(i,r) = pim(i,r) + tgm(i,r);
   1504    
   1505   !FRAZAD:CRP: - nonzero divide added !
   1506   Zerodivide (nonzero_by_zero) default 0;
   1507   Zerodivide (zero_by_zero) default 0;
   1508   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   1509       GMSHR(i,s) # share of imports for gov't hhld at agent's prices #;
   1510   Formula (all,i,TRAD_COMM)(all,s,REG)
   1511       GMSHR(i,s) = VIGA(i,s) / VGA(i,s);
   1512    
   1513   Zerodivide (zero_by_zero) off;
   1514   Zerodivide (nonzero_by_zero) off;
   1515    
   1516   Equation GCOMPRICE
   1517   # government consumption price for composite commodities (HT 42) #
   1518   (all,i,TRAD_COMM)(all,s,REG)
   1519       pg(i,s) =  GMSHR(i,s) * pgm(i,s) + [1 - GMSHR(i,s)] * pgd(i,s);
   1520    
   1521   Equation GHHLDAGRIMP
   1522   # government consumption demand for aggregate imports (HT 43) #
   1523   (all,i,TRAD_COMM)(all,s,REG)
   1524       qgm(i,s) = qg(i,s) + ESUBD(i) * [pg(i,s) - pgm(i,s)];
   1525    
   1526   Equation GHHLDDOM
   1527   # government consumption demand for domestic goods (HT 44) #
   1528   (all,i,TRAD_COMM)(all,s,REG)
   1529       qgd(i,s) = qg(i,s) + ESUBD(i) * [pg(i,s) - pgd(i,s)];
   1530    
   1531   Equation TGCRATIO
   1532   # change in ratio of government consumption tax payments to regional income #
   1533   (all,r,REG)
   1534       100.0 * INCOME(r) * del_taxrgc(r) + TGC(r) * y(r)
   1535           = sum(i,TRAD_COMM,
   1536               VDGA(i,r) * tgd(i,r) + DGTAX(i,r) * [pm(i,r) + qgd(i,r)])
   1537           + sum(i,TRAD_COMM,
   1538               VIGA(i,r) * tgm(i,r) + IGTAX(i,r) * [pim(i,r) + qgm(i,r)]);
   1539    
   1540   ! ECONS: The private hhld module has been rearranged
   1541        to accommodate the GTAP-E approach to nesting and to
   1542        add an energy composite!
