   5704   !<
   5705       A-3. GDP Indices (Value, Price and Quantity)
   5706       --------------------------------------------
   5707   >!
   5708    
   5709   Coefficient (all,r,REG)
   5710       GDP(r) # Gross Domestic Product in region r #;
   5711   Formula (all,s,REG)
   5712       GDP(s)
   5713           = sum(i,TRAD_COMM, VPAD(i,s))
   5714           + sum(i,TRAD_COMM, VGA(i,s))
   5715           + sum(k,CGDS_COMM, VOA(k,s))
   5716           + sum(i,TRAD_COMM, sum(r,REG, VXWD(i,s,r)))
   5717           + sum(m,MARG_COMM, VST(m,s))
   5718           - sum(i,TRAD_COMM, sum(r,REG, VIWS(i,r,s)));
   5719    
   5720   !<
   5721       Gross Domestic Product in region r.  Trade is valued at FOB and CIF
   5722       prices.
   5723   >!
   5724    
   5725   ! ECONS Subsitute VPAD, ppt and qpt!
   5726    
   5727   Variable (all,r,REG)
   5728       vgdp(r) # change in value of GDP #;
   5729   Equation VGDP_r
   5730   # change in value of GDP (HT 70) #
   5731   (all,r,REG)
   5732       GDP(r) * vgdp(r)
   5733           = sum(i,TRAD_COMM, VGA(i,r) * [qg(i,r) + pg(i,r)])
   5734           + sum(i,TRAD_COMM, VPAD(i,r) * [qpt(i,r) + ppt(i,r)])
   5735           + REGINV(r) * [qcgds(r) + pcgds(r)]
   5736           + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * [qxs(i,r,s) + pfob(i,r,s)]))
   5737           + sum(m,MARG_COMM, VST(m,r) * [qst(m,r) + pm(m,r)])
   5738           - sum(i,TRAD_COMM, sum(s,REG,
   5739               VIWS(i,s,r) * [qxs(i,s,r) + pcif(i,s,r)]));
   5740    
   5741   Variable (orig_level=1.0)(all,r,REG)
   5742       pgdp(r) # GDP price index #;
   5743   Equation PGDP_r
   5744   # GDP price index (HT 71) #
   5745   (all,r,REG)
   5746       GDP(r) * pgdp(r)
   5747           = sum(i,TRAD_COMM, VGA(i,r) * pg(i,r))
   5748           + sum(i,TRAD_COMM, VPAD(i,r) * ppt(i,r))
   5749           + REGINV(r) * pcgds(r)
   5750           + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * pfob(i,r,s)))
   5751           + sum(m,MARG_COMM, VST(m,r) * pm(m,r))
   5752           - sum(i,TRAD_COMM, sum(s,REG, VIWS(i,s,r) * pcif(i,s,r)));
   5753    
   5754   Variable (orig_level=GDP)(all,r,REG)
   5755       qgdp(r) # GDP quantity index #;
   5756   Equation QGDP_r
   5757   # GDP quantity index #
   5758   (all,r,REG)
   5759       GDP(r) * qgdp(r)
   5760           = sum(i,TRAD_COMM, VGA(i,r) * qg(i,r))
   5761           + sum(i,TRAD_COMM, VPAD(i,r) * qpt(i,r))
   5762           + REGINV(r) * qcgds(r)
   5763           + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * qxs(i,r,s)))
   5764           + sum(m,MARG_COMM, VST(m,r) * qst(m,r))
   5765           - sum(i,TRAD_COMM, sum(s,REG, VIWS(i,s,r) * qxs(i,s,r)));
   5766   !<
   5767       modified from HT 72 for use with AnalyzeGE
   5768   >!
