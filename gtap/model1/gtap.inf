   5663   !<
   5664       A-1. Factor Price Indices
   5665       -------------------------
   5666   >!
   5667    
   5668   Variable (orig_level=1.0)(all,i,ENDW_COMM)(all,r,REG)
   5669       pfactreal(i,r) # ratio of return to primary factor i to CPI in r #;
   5670   Equation REALRETURN
   5671   # eq'n defines the real rate of return to primary factor i in region r #
   5672   (all,i,ENDW_COMM)(all,s,REG)
   5673       pfactreal(i,s) = pm(i,s) - ppriv(s);
   5674   !<
   5675       This equation defines the real rate of return to primary factor i in
   5676       region r (new).
   5677   >!
   5678    
   5679   Coefficient (all,r,REG)
   5680       VENDWREG(r) # value of primary factors, at mkt prices, by region #;
   5681   Formula (all,r,REG)
   5682       VENDWREG(r) = sum(i,ENDW_COMM, VOM(i,r));
   5683    
   5684   Variable (orig_level=1.0)(all,r,REG)
   5685       pfactor(r) # market price index of primary factors, by region #;
   5686   Equation PRIMFACTPR
   5687   # computes % change in price index of primary factors, by region #
   5688   (all,r,REG)
   5689       VENDWREG(r) * pfactor(r) = sum(i,ENDW_COMM, VOM(i,r) * pm(i,r));
   5690    
   5691   Coefficient
   5692       VENDWWLD # value of primary factors, at mkt prices, worldwide #;
   5693   Formula
   5694       VENDWWLD = sum(r,REG, VENDWREG(r));
   5695   Variable (orig_level=1.0)
   5696       pfactwld # world price index of primary factors #;
   5697   Equation PRIMFACTPRWLD
   5698   # computes % change in global price index of primary factors #
   5699       VENDWWLD * pfactwld = sum(r,REG, VENDWREG(r) * pfactor(r));
   5700    
