   1833    
   1834   !<
   1835       2-4. Energy Nest
   1836       ----------------
   1837   >!
   1838    
   1839   ! biofuels: replace oil-pcts, ethanol and biodiesel with biooil composite !
   1840   !------------------------------------------------------------------------------!
   1841   !electricity is renamed to electricity0 to aggregate all electricities!
   1842    
   1843   Set
   1844       HEGY_COMM # inputs into aggregate hhld energy consumption #
   1845      (coal, oil, gas, electricity0, hbiooil);
   1846    
   1847   Subset
   1848       HEGY_COMM is subset of HHLD_COMM;
   1849    
   1850   Formula (all,r,REG)
   1851       VPAS("henergy",r) = sum(i,HEGY_COMM, VPA(i,r));
   1852   Formula (all,r,REG)
   1853       VPA("henergy",r) = VPAS("henergy",r);
   1854    
   1855   Coefficient
   1856       SIZE_HEGY # size of HEGY_COMM set #;
   1857   Formula
   1858       SIZE_HEGY = sum(i,HEGY_COMM, 1);
   1859    
   1860   Coefficient (all,i,HEGY_COMM)(all,r,REG)
   1861       CSHEGY(i,r)
   1862       # share of i in cost to j of henergy subproduct #;
   1863   Formula (all,i,HEGY_COMM)(all,r,REG: VPAS("henergy",r) = 0)
   1864       CSHEGY(i,r) = 1.0 / SIZE_HEGY;
   1865   Formula (all,i,HEGY_COMM)(all,r,REG: VPAS("henergy",r) > 0)
   1866       CSHEGY(i,r) = VPA(i,r) / VPAS("henergy",r);
   1867    
   1868   Equation HEGYPRICE # price of henergy #
   1869   (all,r,REG)
   1870       pp("henergy",r)
   1871           = sum(k,HEGY_COMM, CSHEGY(k,r)* pp(k,r));
   1872    
   1873   Coefficient (parameter) (all,r,REG)
   1874       ELEGY(r)
   1875       # elasticity of substitution in henergy subconsumption #;
   1876   Read
   1877       ELEGY from file GTAPPARM header "ELEG";
   1878    
   1879   Equation HEGYDEMAND
   1880   # demand for inputs into henergy subconsumption #
   1881   (all,i,HEGY_COMM)(all,r,REG)
   1882       qp(i,r) = qp("henergy",r)
   1883           - ELEGY(r) * [pp(i,r) - pp("henergy",r)];
   1884    
   1885   !<
   1886       2-5. Vegetable Oils Nest
   1887       ------------------------
   1888    
   1889   >!
   1890    
   1891   Set
   1892       HVOL_COMM # inputs into aggregate hhld veg. oils consumption #
   1893      (Vol_Soy1, Vol_Palm1, Vol_Rape1, Vol_Oth1);
   1894    
   1895   Subset
   1896       HVOL_COMM is subset of HHLD_COMM;
   1897    
   1898   Formula (all,r,REG)
   1899       VPAS("hvol",r) = sum(i,HVOL_COMM, VPA(i,r));
   1900   Formula (all,r,REG)
   1901       VPA("hvol",r) = VPAS("hvol",r);
   1902    
   1903   Coefficient
   1904       SIZE_HVOL # size of HVOL_COMM set #;
   1905   Formula
   1906       SIZE_HVOL = sum(i,HVOL_COMM, 1);
   1907    
   1908   Coefficient (all,i,HVOL_COMM)(all,r,REG)
   1909       CSHHVOL(i,r)
   1910       # share of i in cost to j of veg. oils subproduct #;
   1911   Formula (all,i,HVOL_COMM)(all,r,REG: VPAS("hvol",r) = 0)
   1912       CSHHVOL(i,r) = 1.0 / SIZE_HVOL;
   1913   Formula (all,i,HVOL_COMM)(all,r,REG: VPAS("hvol",r) > 0)
   1914       CSHHVOL(i,r) = VPA(i,r) / VPAS("hvol",r);
   1915    
   1916   Equation HVOLPRICE # price of veg. oils #
   1917   (all,r,REG)
   1918       pp("hvol",r)
   1919           = sum(k,HVOL_COMM, CSHHVOL(k,r)* pp(k,r));
   1920    
   1921   Coefficient (parameter) (all,r,REG)
   1922       ELHVOL(r)
   1923       # elasticity of substitution in veg. oils subconsumption #;
   1924   Read
   1925       ELHVOL from file GTAPPARM header "ELHL";
   1926    
   1927   Equation HVOLDEMAND
   1928   # demand for inputs into veg. oils subconsumption #
   1929   (all,i,HVOL_COMM)(all,r,REG)
   1930       qp(i,r) = qp("hvol",r)
   1931           - ELHVOL(r) * [pp(i,r) - pp("hvol",r)];
   1932    
   1933    
   1934   !<
   1935       2-6 Utility from Private Consumption
   1936       -------------------------------------
   1937   >!
   1938    
   1939   ! elasticity of cost wrt utility from private consumption !
   1940    
   1941   Coefficient (all,i,CDE_COMM)(all,r,REG)
   1942       CONSHR(i,r) # share of private hhld consumption devoted to good i in r #;
   1943   Formula (all,i,CDE_COMM)(all,r,REG)
   1944       CONSHR(i,r) = VPA(i,r) / sum(k,CDE_COMM, VPA(k,r));
   1945   Coefficient (parameter)(all,i,CDE_COMM)(all,r,REG)
   1946       INCPAR(i,r)
   1947       # expansion parameter in the CDE minimum expenditure function #;
   1948   Read
   1949       INCPAR from file GTAPPARM header "INCP";
   1950   Coefficient (all,r,REG)
   1951       UELASPRIV(r)
   1952       # elasticity of cost wrt utility from private consumption #;
   1953   Formula (all,r,REG)
   1954       UELASPRIV(r) = sum(i,CDE_COMM, CONSHR(i,r) * INCPAR(i,r));
   1955    
   1956   Equation PHHLDINDEX
   1957   # price index for private consumption expenditure #
   1958   (all,r,REG)
   1959       ppriv(r) = sum(i,CDE_COMM, CONSHR(i,r) * pp(i,r));
   1960    
   1961   Equation PRIVATEU
   1962   # computation of utility from private consumption in r (HT 45) #
   1963   (all,r,REG)
   1964       yp(r) - pop(r) = ppriv(r) + UELASPRIV(r) * up(r);
   1965   !<
   1966       This equation determines private consumption utility for a representative
   1967       household in region r, based on the per capita private expenditure
   1968       function.  (HT 45)
   1969   >!
   1970    
   1971   Coefficient (all,i,CDE_COMM)(all,r,REG)
   1972       XWCONSHR(i,r)
   1973       # expansion-parameter-weighted consumption share #;
   1974   Formula (all,i,CDE_COMM)(all,r,REG)
   1975       XWCONSHR(i,r) = CONSHR(i,r) * INCPAR(i,r) / UELASPRIV(r);
   1976    
   1977   Equation UTILELASPRIV
   1978   # elasticity of expenditure wrt utility from private consumption #
   1979   (all,r,REG)
   1980       uepriv(r)
   1981           = sum(i,CDE_COMM, XWCONSHR(i,r) * [pp(i,r) + qp(i,r) - yp(r)]);
   1982    
   1983    
   1984   !<
   1985       2-7. Allen Partials, Price and Income Elasticities, Composite Demand
   1986       --------------------------------------------------------------------
   1987   >!
   1988    
   1989   Coefficient (parameter)(all,i,CDE_COMM)(all,r,REG)
   1990       SUBPAR(i,r)
   1991       # substitution parameter in CDE minimum expenditure function #;
   1992   Read
   1993       SUBPAR from file GTAPPARM header "SUBP";
   1994    
   1995   !<
   1996       (HT F1 - F3)
   1997   >!
   1998   Coefficient (all,i,CDE_COMM)(all,r,REG)
   1999       ALPHA(i,r) # 1 - sub. parameter in the CDE minimum expenditure function #;
   2000   Formula (all,i,CDE_COMM)(all,r,REG)
   2001       ALPHA(i,r) = 1 - SUBPAR(i,r);
   2002    
   2003   Zerodivide (nonzero_by_zero) default 1;
   2004   Zerodivide (zero_by_zero) default 1;
   2005   Coefficient (all,i,CDE_COMM)(all,k,CDE_COMM)(all,r,REG)
   2006       APE(i,k,r) # Allen partial elst. of sub. between composite i and k in r #;
   2007   Formula (all,i,CDE_COMM)(all,k,CDE_COMM)(all,r,REG)
   2008       APE(i,k,r)
   2009           = ALPHA(i,r) + ALPHA(k,r) - sum(n,CDE_COMM, CONSHR(n,r) * ALPHA(n,r));
   2010   Formula (all,i,CDE_COMM)(all,r,REG)
   2011       APE(i,i,r)
   2012           = 2.0 * ALPHA(i,r)
   2013           - sum(n,CDE_COMM, CONSHR(n,r) * ALPHA(n,r))
   2014           - ALPHA(i,r) / CONSHR(i,r);
   2015   Zerodivide (nonzero_by_zero) off;
   2016   Zerodivide (zero_by_zero) off;
   2017    
   2018   Coefficient (all,i,CDE_COMM)(all,r,REG)
   2019       EY(i,r) # income elast. of private hhld demand for i in r (HT F4) #;
   2020   Formula (all,i,CDE_COMM)(all,r,REG)
   2021       EY(i,r)
   2022           =   [1.0 / sum(n,CDE_COMM, CONSHR(n,r) * INCPAR(n,r))]
   2023               * [INCPAR(i,r) * [1.0 - ALPHA(i,r)]
   2024                   + sum(n,CDE_COMM, CONSHR(n,r) * INCPAR(n,r) * ALPHA(n,r))]
   2025           + [ALPHA(i,r) - sum(n,CDE_COMM, CONSHR(n,r) * ALPHA(n,r))];
   2026    
   2027   Coefficient (all,i,CDE_COMM)(all,k,CDE_COMM)(all,r,REG)
   2028       EP(i,k,r)
   2029       # uncomp. elast. of private hhld demand for i wrt price of k in r (HT F5) #;
   2030   Formula (all,i,CDE_COMM)(all,k,CDE_COMM)(all,r,REG)
   2031       EP(i,k,r) = 0;
   2032   Formula (all,i,CDE_COMM)(all,k,CDE_COMM)(all,r,REG)
   2033       EP(i,k,r) = [APE(i,k,r) - EY(i,r)] * CONSHR(k,r);
   2034    
   2035   Equation PRIVDMNDS
   2036   # private consumption demands for composite commodities (HT 46) #
   2037   (all,i,CDE_COMM)(all,r,REG)
   2038       qp(i,r) - pop(r)
   2039           = sum(k,CDE_COMM, EP(i,k,r) * pp(k,r)) + EY(i,r) * [yp(r) - pop(r)];
   2040   !<
   2041       Private consumption demands for composite commodities.  Demand system is
   2042       on a per capita basis.  Here, yp(r) - pop(r) is % change in per capita
   2043       income.  (HT 46)
   2044   >!
