   5781       B-3. Regional Household Shadow Demand System
   5782       B-4. Equivalent Variation
   5783    
   5784       This appendix calculates equivalent variation "EV" and world equivalent
   5785       variation, "WEV", by determining the income "yev" that would be required
   5786       to achieve the current actual utility level "u" in a shadow demand system
   5787       in which prices are fixed.
   5788    
   5789       Section B-2 calculates the utility elasticity of private consumption
   5790       expenditure, "ueprivev", within a shadow demand system for private
   5791       consumption, for use in section B-3.  B-3 calculates private consumption
   5792       expenditure "ypev" for use in B-2, and regional income "yev" for use in
   5793       B-4, within a shadow demand system for the regional household.  B-4
   5794       calculates "EV" and "WEV".
   5795   >!
   5796    
   5797    
   5798   !<
   5799       B-0. Appendix-Specific Variables and Coefficients
   5800       -------------------------------------------------
   5801    
   5802       only used in this Equivalent Variation section of the Summary Indices
   5803       appendix
   5804   >!
   5805    
   5806   Variable (all,r,REG)
   5807       uelasev(r)
   5808       # elasticity of cost of utility wrt utility, for EV calc. #;
   5809   Variable (all,r,REG)
   5810       ueprivev(r)
   5811       # utility elasticity of private consn expenditure, for EV calc. #;
   5812   Variable (all,r,REG)
   5813       ugev(r) # per capita utility from gov't expend., for EV calc. #;
   5814   Variable (all,r,REG)
   5815       upev(r) # per capita utility from private expend., for EV calc. #;
   5816   Variable (all,r,REG)
   5817       qsaveev(r) # total quantity of savings demanded, for EV calc. #;
   5818   Variable (all,r,REG)
   5819       yev(r) # regional household income in region r, for EV calc. #;
   5820   Variable (all,r,REG)
   5821       ypev(r)
   5822       # private consumption expenditure in region r, for EV calc. #;
   5823   Variable (all,r,REG)
   5824       ygev(r)
   5825       # government consumption expenditure in region r, for EV calc. #;
   5826    
   5827   Coefficient (all,r,REG)
   5828       INCOMEEV(r) # regional income, for EV calc. #;
   5829   Formula (initial) (all,r,REG)
   5830       INCOMEEV(r) = INCOME(r);
   5831   Update (all,r,REG)
   5832       INCOMEEV(r) = yev(r);
   5833    
   5834   Coefficient (all,r,REG)
   5835       UTILPRIVEV(r) # utility from private consumption, for EV calcs #;
   5836   Formula (initial) (all,r,REG)
   5837       UTILPRIVEV(r) = UTILPRIV(r);
   5838   Update (all,r,REG)
   5839       UTILPRIVEV(r) = upev(r);
   5840    
   5841   Coefficient (all,r,REG)
   5842       UTILGOVEV(r) # utility from private consumption, for EV calcs #;
   5843   Formula (initial) (all,r,REG)
   5844       UTILGOVEV(r) = UTILGOV(r);
   5845   Update (all,r,REG)
   5846       UTILGOVEV(r) = ugev(r);
   5847    
   5848   Coefficient (all,r,REG)
   5849       UTILSAVEEV(r) # utility from private consumption, for EV calcs #;
   5850   Formula (initial) (all,r,REG)
   5851       UTILSAVEEV(r) = UTILSAVE(r);
   5852   Update (change) (all,r,REG)
   5853       UTILSAVEEV(r) = [[qsaveev(r) - pop(r)] / 100] * UTILSAVEEV(r);
   5854    
   5855    
   5856   !<
   5857       B-1. Government Consumption Shadow Demand System
   5858       ------------------------------------------------
   5859   >!
   5860    
   5861   Equation GOVUSHD
   5862   # utility from government consumption in r #
   5863   (all,r,REG)
   5864       ygev(r) - pop(r) = ugev(r);
   5865    
   5866    
   5867   !<
   5868       B-2. Private Consumption Shadow Demand System
   5869       ---------------------------------------------
   5870   >!
   5871    
   5872   Variable (all,i,CDE_COMM)(all,r,REG)
   5873       qpev(i,r)
   5874       # private hhld demand for commodity i in region r, for EV calc. #;
   5875    
   5876   Coefficient (all,i,CDE_COMM)(all,r,REG)
   5877       VPAEV(i,r)
   5878       # private hhld expend. on i in r valued at agent's prices, for EV calc. #;
   5879   Formula (initial) (all,i,CDE_COMM)(all,r,REG)
   5880       VPAEV(i,r) = VPA(i,r);
   5881   Update (all,i,CDE_COMM)(all,r,REG)
   5882       VPAEV(i,r) = qpev(i,r);
   5883    
   5884   Coefficient (all,r,REG)
   5885       VPAREGEV(r) # private consumption expenditure in region r, for EV calc. #;
   5886   Formula (all,r,REG)
   5887       VPAREGEV(r) = sum(i,CDE_COMM, VPAEV(i,r));
   5888   !<
   5889       VPAREGEV should agree with PRIVEXPEV.
   5890   >!
   5891    
   5892   Coefficient (all,i,CDE_COMM)(all,r,REG)
   5893       CONSHREV(i,r)
   5894       # share of private hhld consn devoted to good i in r, for EV calc. #;
   5895   Formula (all,i,CDE_COMM)(all,r,REG)
   5896       CONSHREV(i,r) = VPAEV(i,r) / VPAREGEV(r);
   5897   Coefficient (all,i,CDE_COMM)(all,r,REG)
   5898       EYEV(i,r)
   5899       # expend. elast. of private hhld demand for i in r, for EV calc. #;
   5900   Formula (all,i,CDE_COMM)(all,r,REG)
   5901       EYEV(i,r)
   5902           = [1.0 / sum(n,CDE_COMM, CONSHREV(n,r) * INCPAR(n,r))]
   5903           * [INCPAR(i,r) * [1.0 - ALPHA(i,r)]
   5904               + sum(n,CDE_COMM, CONSHREV(n,r) * INCPAR(n,r) * ALPHA(n,r))]
   5905           + ALPHA(i,r)
   5906           - sum(n,CDE_COMM, CONSHREV(n,r) * ALPHA(n,r));
   5907    
   5908   Equation PRIVDMNDSEV
   5909       # private hhld demands for composite commodities, for EV calc. #
   5910       (all,i,CDE_COMM)(all,r,REG)
   5911       qpev(i,r) - pop(r) = EYEV(i,r) * [ypev(r) - pop(r)];
   5912   !<
   5913       Prices are held constant for the EV calculation and so do not appear here.
   5914   >!
   5915    
   5916   Coefficient (all,r,REG)
   5917       UELASPRIVEV(r)
   5918       # elast. of cost wrt utility from private consn, for EV calc. #;
   5919   Formula (all,r,REG)
   5920       UELASPRIVEV(r) = sum(i,CDE_COMM, CONSHREV(i,r) * INCPAR(i,r));
   5921    
   5922   Equation PRIVATEUEV
   5923   # computation of utility from private consumption in r (HT 45) #
   5924   (all,r,REG)
   5925       ypev(r) - pop(r) = UELASPRIVEV(r) * upev(r);
   5926    
   5927   Coefficient (all,i,CDE_COMM)(all,r,REG)
   5928       XWCONSHREV(i,r)
   5929       # expansion-parameter-weighted consumption share, for EV calc. #;
   5930   Formula (all,i,CDE_COMM)(all,r,REG)
   5931       XWCONSHREV(i,r) = CONSHREV(i,r) * INCPAR(i,r) / UELASPRIVEV(r);
   5932    
   5933   Equation UTILELASPRIVEV
   5934       # elasticity of cost wrt utility from private consn, for EV calc. #
   5935       (all,r,REG)
   5936       ueprivev(r)
   5937           = sum(i,CDE_COMM, XWCONSHREV(i,r) * [qpev(i,r) - ypev(r)]);
   5938   !<
   5939       Prices are held constant for the EV calculation and so do not appear here.
   5940   >!
   5941    
   5942    
   5943   !<
   5944    
   5945       B-3. Regional Household Shadow Demand System
   5946       --------------------------------------------
   5947   >!
   5948    
   5949   Variable (all,r,REG)
   5950       ysaveev(r) # NET savings expenditure, for EV calc. #;
   5951    
   5952   Coefficient (all,r,REG)
   5953       PRIVEXPEV(r)
   5954       # private consumption expenditure in region r, for EV calc. #;
   5955   Formula (initial) (all,r,REG)
   5956       PRIVEXPEV(r) = PRIVEXP(r);
   5957   Update (all,r,REG)
   5958       PRIVEXPEV(r) = ypev(r);
   5959   !<
   5960       PRIVEXPEV should agree with VPAREGEV.
   5961   >!
   5962    
   5963   Coefficient (all,r,REG)
   5964       GOVEXPEV(r)
   5965       # government consumption expenditure in region r, for EV calc. #;
   5966   Formula (initial) (all,r,REG)
   5967       GOVEXPEV(r) = GOVEXP(r);
   5968   Update (all,r,REG)
   5969       GOVEXPEV(r) = ygev(r);
   5970    
   5971   Coefficient (all,r,REG)
   5972       SAVEEV(r)
   5973       # saving in region r, for EV calc. #;
   5974   Formula (initial) (all,r,REG)
   5975       SAVEEV(r) = SAVE(r);
   5976   Update (all,r,REG)
   5977       SAVEEV(r) = ysaveev(r);
   5978    
   5979   Coefficient (all,r,REG)
   5980       XSHRPRIVEV(r)
   5981       # private expenditure share in regional income, for EV calc. #;
   5982   Formula (all,r,REG)
   5983       XSHRPRIVEV(r) = PRIVEXPEV(r) / INCOMEEV(r);
   5984    
   5985   Coefficient (all,r,REG)
   5986       XSHRGOVEV(r)
   5987       # government expenditure share in regional income, for EV calc. #;
   5988   Formula (all,r,REG)
   5989       XSHRGOVEV(r) = GOVEXPEV(r) / INCOMEEV(r);
   5990    
   5991   Coefficient (all,r,REG)
   5992       XSHRSAVEEV(r) # saving share in regional income, for EV calc. #;
   5993   Formula (all,r,REG)
   5994       XSHRSAVEEV(r) = SAVEEV(r) / INCOMEEV(r);
   5995    
   5996   Variable (all,r,REG)
   5997       dpavev(r) # average distribution parameter shift, for EV calc. #;
   5998   Equation DPARAVEV
   5999   # average distribution parameter shift, for EV calc. #
   6000       (all,r,REG)
   6001       dpavev(r)
   6002           = XSHRPRIVEV(r) * dppriv(r)
   6003           + XSHRGOVEV(r) * dpgov(r)
   6004           + XSHRSAVEEV(r) * dpsave(r);
   6005    
   6006   Equation UTILITELASTICEV
   6007       # elasticity of cost of utility wrt utility, for EV calc. #
   6008       (all,r,REG)
   6009       uelasev(r) = XSHRPRIV(r) * ueprivev(r) - dpavev(r);
   6010    
   6011   Equation PCONSEXPEV
   6012   # private consumption expenditure, for EV calc. #
   6013       (all,r,REG)
   6014       ypev(r) - yev(r) = -[ueprivev(r) - uelasev(r)] + dppriv(r);
   6015    
   6016   Equation GOVCONSEXPEV
   6017   # government consumption expenditure #
   6018   (all,r,REG)
   6019       ygev(r) - yev(r) = uelasev(r) + dpgov(r);
   6020    
   6021   Equation SAVINGEV
   6022   # saving #
   6023   (all,r,REG)
   6024       ysaveev(r) - yev(r) = uelasev(r) + dpsave(r);
   6025    
   6026   Equation SAVEUEV
   6027   # saving #
   6028   (all,r,REG)
   6029       qsaveev(r) = ysaveev(r);
   6030    
   6031   !<
   6032       Note that because psave doesn't change, qsaveev moves with ysaveev.
   6033   >!
   6034    
   6035   Coefficient (all,r,REG)
   6036       UTILELASEV(r)
   6037       # elasticity of cost of utility wrt utility, for EV calc. #;
   6038   Formula (initial) (all,r,REG)
   6039       UTILELASEV(r) = UTILELAS(r);
   6040   Update (all,r,REG)
   6041       UTILELASEV(r) = uelasev(r);
   6042    
   6043   Equation INCOME_EQUIV
   6044   # equivalent income #
   6045   (all,r,REG)
   6046       u(r)
   6047           = au(r)
   6048           + DPARPRIV(r) * loge(UTILPRIVEV(r)) * dppriv(r)
   6049           + DPARGOV(r) * loge(UTILGOVEV(r)) * dpgov(r)
   6050           + DPARSAVE(r) * loge(UTILSAVEEV(r)) * dpsave(r)
   6051           + [1.0 / UTILELASEV(r)] * [yev(r) - pop(r)];
   6052    
   6053    
   6054   !<
   6055       B-4. Equivalent Variation
   6056       -------------------------
   6057   >!
   6058    
   6059   Variable (change)(all,r,REG)
   6060       EV(r) # equivalent variation, $ US million #;
   6061   Equation EVREG
   6062   # regional EV (HT 67) #
   6063   (all,r,REG)
   6064       EV(r) = [INCOMEEV(r) / 100] * yev(r);
   6065    
   6066   Variable (change)
   6067       WEV # equivalent variation for the world #;
   6068   Equation EVWLD
   6069   # EV for the world (HT 68) #
   6070       WEV = sum(r,REG, EV(r));
   6071    
   6072    
   6073   !<
   6074       ------------------------
   6075       C. Welfare Decomposition
   6076       ------------------------
   6077       See GTAP Technical Paper No. 5 for derivation and interpretation.
   6078   >!
   6079    
   6080   Coefficient (all,r,REG)
   6081       EVSCALFACT(r) # equivalent variation scaling factor #;
   6082   Formula (all,r,REG)
   6083       EVSCALFACT(r) = [UTILELASEV(r) / UTILELAS(r)] * [INCOMEEV(r) / INCOME(r)];
   6084    
   6085   Coefficient (all,m,MARG_COMM)(all,s,REG)
   6086       VTMD(m,s) # aggregate value of svce m in shipments to s #;
   6087   Formula (all,m,MARG_COMM)(all,s,REG)
   6088       VTMD(m,s) = sum(i,TRAD_COMM, sum(r,REG, VTMFSD(m,i,r,s)));
   6089    
   6090   Variable (linear,change)(all,r,REG)
   6091       EV_ALT(r) # regional EV computed in alternative way #;
   6092    
   6093   !<<add new coproducts!
   6094   Equation EV_DECOMPOSITION
   6095   # decomposition of Equivalent Variation #
   6096   (all,r,REG)
   6097           EV_ALT(r)
   6098           = -[0.01 * UTILELASEV(r) * INCOMEEV(r)]
   6099           * [DPARPRIV(r) * loge(UTILPRIVEV(r) / UTILPRIV(r)) * dppriv(r)
   6100               + DPARGOV(r) * loge(UTILGOVEV(r) / UTILGOV(r)) * dpgov(r)
   6101               + DPARSAVE(r) * loge(UTILSAVEEV(r) / UTILSAVE(r)) * dpsave(r)]
   6102           + [0.01 * EVSCALFACT(r)]
   6103           * [
   6104               + sum(i,NSAV_COMM, PTAX(i,r) * [qo(i,r) - pop(r)])
   6105               + sum(i,ENDW_COMM, sum(j,ALL_INDS,
   6106                     ETAX(i,j,r) * [qfe(i,j,r) - pop(r)]))
   6107               + sum(j,ALL_INDS, sum(i,TRAD_COMM,
   6108                     [IFTAX(i,j,r) ] * [qfm(i,j,r) - pop(r)]))
   6109               + sum(j,ALL_INDS, sum(i,TRAD_COMM,
   6110                     [DFTAX(i,j,r) ] * [qfd(i,j,r) - pop(r)]))
   6111               + sum(i,TRAD_COMM,
   6112                   [IPTAX(i,r) ] * [qpm(i,r) - pop(r)])
   6113               + sum(i,TRAD_COMM,
   6114                   [DPTAX(i,r) ] * [qpd(i,r) - pop(r)])
   6115               + sum(i,TRAD_COMM,
   6116                   [IGTAX(i,r) ] * [qgm(i,r) - pop(r)])
   6117               + sum(i,TRAD_COMM,
   6118                   [DGTAX(i,r) ] * [qgd(i,r) - pop(r)])
   6119               + sum(i,TRAD_COMM, sum(s,REG, XTAXD(i,r,s) * [qxs(i,r,s) - pop(r)]))
   6120               + sum(i,TRAD_COMM, sum(s,REG, MTAX(i,s,r) * [qxs(i,s,r) - pop(r)]))
   6121               + sum(i,ENDW_COMM, VOA(i,r) * [qo(i,r) - pop(r)])
   6122               - VDEP(r) * [kb(r) - pop(r)]
   6123    
   6124               + sum(i,NETBD_INDS,VOA(i,r) * ao(i,r))
   6125               + sum(k,ETHA_COMM,VOA(k,r))* ao("EthanolC",r)
   6126               + sum(k,ETHAS_COMM,VOA(k,r))* ao("EthanolS",r)
   6127               + sum(k,BIOD_COMM,VOA(k,r))* ao("Vol_Soy",r)
   6128               + sum(k,BIOP_COMM,VOA(k,r))* ao("Vol_Palm",r)
   6129               + sum(k,BIOR_COMM,VOA(k,r))* ao("Vol_Rape",r)
   6130               + sum(k,BIOC_COMM,VOA(k,r))* ao("Vol_Oth",r)
   6131               + sum(k,GRNATJ_COMM,VOA(k,r))* ao("GRNATJ",r)
   6132               + sum(k,GRNETJ_COMM,VOA(k,r))* ao("GRNETJ",r)
   6133               + sum(k,SUGATJ_COMM,VOA(k,r))* ao("SUGATJ",r)
   6134               + sum(k,SUGETJ_COMM,VOA(k,r))* ao("SUGETJ",r)
   6135               + sum(k,SUGSIP_COMM,VOA(k,r))* ao("SUGSIP",r)
   6136               + sum(k,CAMHRJ_COMM,VOA(k,r))* ao("CAMHRJ",r)
   6137               + sum(k,CRNHRJ_COMM,VOA(k,r))* ao("CRNHRJ",r)
   6138    
   6139               + sum(j,ALL_INDS, sum(i,FIRM_COMM, VFA(i,j,r) * af(i,j,r)))
   6140               + sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(s,REG,
   6141                   VTMFSD(m,i,s,r) * atmfsd(m,i,s,r))))
   6142               + sum(i,TRAD_COMM, sum(s,REG, VIMS(i,s,r) * ams(i,s,r)))
   6143               + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * pfob(i,r,s)))
   6144               + sum(m,MARG_COMM, VST(m,r) * pm(m,r))
   6145               + NETINV(r) * pcgds(r)
   6146               - sum(i,TRAD_COMM, sum(s,REG, VXWD(i,s,r) * pfob(i,s,r)))
   6147               - sum(m,MARG_COMM, VTMD(m,r) * pt(m))
   6148               - SAVE(r) * psave(r)]
   6149           + 0.01 * INCOMEEV(r) * pop(r);
   6150    
   6151   Variable (linear,change)
   6152       WEV_ALT # expression for WEV computed in alternative way #;
   6153   Equation WORLDEV
   6154   # Equivalent Variation for the world #
   6155       WEV_ALT = sum(r,REG, EV_ALT(r));
   6156    
   6157    
   6158   Variable (linear,change) (all,r,REG)
   6159       CNTdpar(r) # contribution to EV of change in distribution parameters #;
   6160   Equation CNT_WEV_dpar
   6161   (all,r,REG)
   6162       CNTdpar(r)
   6163           = -0.01 * UTILELASEV(r) * INCOMEEV(r)
   6164           * [DPARPRIV(r) * loge(UTILPRIVEV(r) / UTILPRIV(r)) * dppriv(r)
   6165               + DPARGOV(r) * loge(UTILGOVEV(r) / UTILGOV(r)) * dpgov(r)
   6166               + DPARSAVE(r) * loge(UTILSAVEEV(r) / UTILSAVE(r)) * dpsave(r)];
   6167    
   6168   Variable (linear,change) (all,r,REG)
   6169       CNTpopr(r) # contribution to EV in region r of change in population #;
   6170   Equation CONT_EV_pop
   6171   (all,r,REG)
   6172       CNTpopr(r) = 0.01 * INCOMEEV(r) * pop(r);
   6173    
   6174   Variable (linear,change) (all,r,REG)
   6175       CNTqor(r) # contribution to EV in region r of output changes #;
   6176   Equation CONT_EV_qor
   6177   (all,r,REG)
   6178       CNTqor(r)
   6179           = sum(i,NSAV_COMM,
   6180               0.01 * EVSCALFACT(r) * PTAX(i,r) * [qo(i,r) - pop(r)]);
   6181    
   6182   Variable (linear,change) (all,i,NSAV_COMM)(all,r,REG)
   6183       CNTqoir(i,r)
   6184       # contribution to EV of changes in output of NSAV_COMM i in reg. r #;
   6185   Equation CONT_EV_qoir
   6186   (all,i,NSAV_COMM)(all,r,REG)
   6187       CNTqoir(i,r) = PTAX(i,r) * [0.01 * EVSCALFACT(r)] * [qo(i,r) - pop(r)];
   6188    
   6189   Variable (linear,change) (all,r,REG)
   6190       CNTqfer(r)
   6191       # cont. to EV of changes in use of all ENDW_COMM in all ind. in reg. r #;
   6192   Equation CONT_EV_qfer
   6193   (all,r,REG)
   6194       CNTqfer(r)
   6195           = sum(i,ENDW_COMM, sum(j,ALL_INDS,
   6196               ETAX(i,j,r) * [0.01 * EVSCALFACT(r)] * [qfe(i,j,r) - pop(r)]));
   6197    
   6198   Variable (linear,change) (all,i,ENDW_COMM)(all,r,REG)
   6199       CNTqfeir(i,r)
   6200       # contribution to EV of changes in use of ENDW_COMM i in all ind. in r #;
   6201   Equation CONT_EV_qfeir
   6202   (all,i,ENDW_COMM)(all,r,REG)
   6203       CNTqfeir(i,r)
   6204           = sum(j,ALL_INDS,
   6205               ETAX(i,j,r) * [0.01 * EVSCALFACT(r)] * [qfe(i,j,r) - pop(r)]);
   6206    
   6207   Variable (linear,change) (all,i,ENDW_COMM)(all,j,ALL_INDS)(all,r,REG)
   6208       CNTqfeijr(i,j,r)
   6209       # cont. to EV of changes in use of ENDW_COMM i in ind. j of reg. r #;
   6210   Equation CONT_EV_qfeijr
   6211   (all,i,ENDW_COMM)(all,j,ALL_INDS)(all,r,REG)
   6212       CNTqfeijr(i,j,r)
   6213           = ETAX(i,j,r) * [0.01 * EVSCALFACT(r)] * [qfe(i,j,r) - pop(r)];
   6214    
   6215   Variable (linear,change) (all,r,REG)
   6216       CNTqfmr(r)
   6217       # cont. to EV of changes in use of imported int. in all ind. in reg. r #;
   6218   Equation CONT_EV_qfmr
