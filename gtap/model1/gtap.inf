   1353       TOUTNBIO(r) # ordinary output tax of non-biooil products #;
   1354   Formula (all,r,REG)
   1355       TOUTNBIO(r) = sum(i,NBIOOIL_COMM, PTAX(i,r));
   1356    
   1357   Coefficient (all,r,REG)
   1358       TOUTBIO(r) # ordinary output tax on biooil products #;
   1359   Formula (all,r,REG)
   1360       TOUTBIO(r) = sum(i,BIOOIL_COMM, PTAX(i,r));
   1361    
   1362   Coefficient (all,r,REG)
   1363       TOUT(r) # production tax payments in r #;
   1364    
   1365   Formula (all,r,REG)
   1366    
   1367       TOUT(r) = TOUTNBIO(r) + TOUTBIO(r);
   1368    
   1369   !Farzad 2015: Notice that TOUT was originaly defined over PROD_COMM
   1370    in the following original formulat. But in the obove formual I defined it
   1371   over TRAD_COMM which is the sum of biofuels and non biofules
   1372    
   1373   Formula (all,r,REG)
   1374       TOUT(r) = sum(i,PROD_COMM, PTAX(i,r));!
   1375    
   1376    
   1377   ! Farzad 2015: BIOFUELS end !
   1378    
   1379   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1380       XTAXD(i,r,s) # tax on exports of good i from source r to destination s #;
   1381   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1382       XTAXD(i,r,s) = VXWD(i,r,s) - VXMD(i,r,s);
   1383   Coefficient (all,r,REG)
   1384       TEX(r) # export tax payments in r #;
   1385   Formula (all,r,REG)
   1386       TEX(r) = sum(i,TRAD_COMM, sum(s,REG, XTAXD(i,r,s)));
   1387    
   1388   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1389       MTAX(i,r,s) # tax on imports of good i from source r in destination s #;
   1390   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1391       MTAX(i,r,s) = VIMS(i,r,s) - VIWS(i,r,s);
   1392   Coefficient (all,r,REG)
   1393       TIM(r) # import tax payments in r #;
   1394   Formula (all,r,REG)
   1395       TIM(r) = sum(i,TRAD_COMM, sum(s,REG, MTAX(i,s,r)));
   1396    
