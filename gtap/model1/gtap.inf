   3912   !<
   3913       3-11. Output Taxation
   3914       ---------------------
   3915   >!
   3916    
   3917   ! Farzad 2015: BIOFUELS: Separate out the biofuels component of this to permit
   3918       implementation of a RFS by using a subsidy on output of biofuels !
   3919    
   3920   Equation OUTPUTPRICES
   3921   # Eq'n links pre- and post-tax supply prices for all inductries (HT 15) #
   3922   (all,i,PROD_COMM)(all,r,REG)
   3923       ps(i,r) = to(i,r) + pm(i,r);
   3924    
   3925   !<
   3926       This equation links pre- and post-tax supply prices for all industries.
   3927       This captures the effect of output taxes.  TO(i,r) < 1 in the case of a
   3928       tax.  (HT 15)
   3929   >!
   3930    
   3931   Variable (change) (all,r,REG)
   3932       del_taxroutnbio(r) # change in ratio output tax on nonbiooil to INCOME #;
   3933   Variable (change) (all,r,REG)
   3934       del_taxroutbio(r) # change in ratio output tax on biooil to INCOME #;
   3935    
   3936   Equation TOUTRATIONBIO
   3937   # change in ratio of output tax payments to regional income #
   3938   (all,r,REG)
   3939       100.0 * INCOME(r) * del_taxroutnbio(r) + TOUTNBIO(r) * y(r)
   3940           = sum(i,NBIOOIL_COMM,
   3941               VOA(i,r) * [-to(i,r)] + PTAX(i,r) * [pm(i,r) + qo(i,r)]);
   3942    
   3943   Equation TOUTRATIOBIO
   3944   # change in ratio of output tax payments to regional income #
   3945   (all,r,REG)
   3946       100.0 * INCOME(r) * del_taxroutbio(r) + TOUTBIO(r) * y(r)
   3947           = sum(i,BIOOIL_COMM,
   3948               VOA(i,r) * [-to(i,r)] + PTAX(i,r) * [pm(i,r) + qo(i,r)]);
   3949    
   3950   Equation TOUTRATIO
   3951   # change in ratio of private consumption tax payments to regional income #
   3952   (all,r,REG)
   3953       del_taxrout(r)
   3954           = del_taxroutnbio(r) + del_taxroutbio(r);
   3955    
   3956    ! Farzad 2015: BIOFUELS end !
   3957    
   3958   !<
   3959       ---------------------------------------
   3960       4. Investment, Global Bank, and Savings
   3961       ---------------------------------------
   3962    
   3963       Capital stock and rate of return equations follow.  They correspond to the
   3964       Investment Equations of Table 8 in Hertel and Tsigas.
   3965    
   3966       4-0. Module-Specific Variables
   3967       4-1. Equations of Notational Convenience
   3968       4-2. Rate of Return Equations
   3969       4-3. Capital Accumulation
   3970       4-4. Global Bank
   3971       4-5. Price Index of Aggregate Global Composite Capital Goods
   3972       4-6. Price of Saving
   3973   >!
   3974    
   3975    
   3976   !<
   3977       4-0. Module-Specific Variables
   3978       ------------------------------
   3979    
   3980       only used in this Investment, Global Bank and Savings module
   3981   >!
   3982    
   3983   Variable (all,r,REG)
   3984       rental(r) # rental rate on capital = ps("capital",r) #;
   3985   Variable (all,r,REG)
   3986       ke(r) # end-of-period capital stock in r #;
   3987   Variable (all,r,REG)
   3988       rore(r) # expected net rate of return on capital stock in r #;
   3989   Variable (all,r,REG)
   3990       rorc(r) # current net rate of return on capital stock in r #;
   3991   Variable (all,r,REG)
   3992       qcgds(r) # output of capital goods sector = qo("cgds",r) #;
   3993    
   3994    
   3995   !<
   3996       4-1. Equations of Notational Convenience
   3997       ----------------------------------------
   3998   >!
   3999    
   4000   Variable (all,r,REG)
   4001       ksvces(r) # capital services = qo("capital",r) #;
   4002   Equation KAPSVCES
   4003   # eq'n defines a variable for capital services (HT 52) #
   4004   (all,r,REG)
   4005       ksvces(r)
   4006           = sum(h,ENDWC_COMM, [VOA(h,r) / sum(k,ENDWC_COMM, VOA(k,r))] * qo(h,r));
   4007   !<
   4008       This equation defines a variable for capital services, for convenience.
   4009       (There is really only one capital services item.) (HT 52)
   4010   >!
   4011    
   4012   Equation KAPRENTAL
   4013   # eq'n defines a variable for capital rental rate (HT 53) #
   4014   (all,r,REG)
   4015       rental(r)
   4016           = sum(h,ENDWC_COMM, [VOA(h,r) / sum(k,ENDWC_COMM, VOA(k,r))] * ps(h,r));
   4017    
   4018   Equation CAPGOODS
   4019   # eq'n defines a variable for gross investment (HT 54) #
   4020   (all,r,REG)
   4021       qcgds(r)
   4022           = sum(h,CGDS_COMM, [VOA(h,r) / REGINV(r)] * qo(h,r));
   4023   !<
   4024       This equation defines a variable for gross investment, for convenience.
   4025       There is really only one capital goods item.
   4026   >!
   4027    
   4028   Equation PRCGOODS
   4029   # eq'n defines the price of cgds (HT 55) #
   4030   (all,r,REG)
   4031       pcgds(r) = sum(h,CGDS_COMM, [VOA(h,r) / REGINV(r)] * ps(h,r));
   4032    
   4033   Equation KBEGINNING
   4034   # associates change in cap. services w/ change in cap. stock (HT 56) #
   4035   (all,r,REG)
   4036       kb(r) = ksvces(r);
   4037   !<
   4038       This equation associates any change in capital services during the period
   4039       with a change in capital stock.  Full capacity utilization is assumed.
   4040   >!
   4041    
   4042   Coefficient (ge 0)(all,r,REG)
   4043       VKB(r) # value of beginning-of-period capital stock in region r #;
   4044   Update (all,r,REG)
   4045       VKB(r) = pcgds(r) * kb(r);
   4046   Read
   4047       VKB from file GTAPDATA header "VKB";
   4048   !<
   4049       VKB(r) is used only in this sub-module, so its data are read here.  The
   4050       associated variables, pcgds(r) and kb(r), are used across modules, so they
   4051       have been defined as common variables.
   4052   >!
   4053    
   4054   Coefficient (all,r,REG)
   4055       INVKERATIO(r)
   4056       # ratio of gross investment to end-of-period capital stock in r #;
   4057   Formula (all,r,REG)
   4058       INVKERATIO(r) = REGINV(r) / [VKB(r) + NETINV(r)];
   4059    
   4060   Equation KEND
   4061   # Ending capital stock equals beginning stock plus net investment.  (HT 10) #
   4062   (all,r,REG)
   4063       ke(r) = INVKERATIO(r) * qcgds(r) + [1.0 - INVKERATIO(r)] * kb(r);
   4064    
   4065    
   4066   !<
   4067       4-2. Rate of Return Equations
   4068       -----------------------------
   4069   >!
   4070    
   4071   Coefficient (all,r,REG)
   4072       GRNETRATIO(r) # ratio of GROSS/NET rates of return on capital in r #;
   4073   Formula (all,r,REG)
   4074       GRNETRATIO(r)
   4075           = sum(h,ENDWC_COMM, VOA(h,r))
   4076           / [sum(h,ENDWC_COMM, VOA(h,r)) - VDEP(r)];
   4077   !<
   4078       NOTE: VOA("capital",r) is GROSS returns to capital.
   4079   >!
   4080   Equation RORCURRENT
   4081   # current rate of return on capital in region r (HT 57) #
   4082   (all,r,REG)
   4083       rorc(r) = GRNETRATIO(r) * [rental(r) - pcgds(r)];
   4084    
   4085   Coefficient (parameter)(all,r,REG)
   4086       RORFLEX(r)
   4087       # flexibility of expected net ROR on capital stock in r wrt investment #;
   4088   Read
   4089       RORFLEX from file GTAPPARM header "RFLX";
   4090   !<
   4091       RORFLEX is the flexibility of the expected net rate of return on the
   4092       capital stock, in region r, with respect to investment.  If a region's
   4093       capital stock increases by 1%, then it is expected that the net rate of
   4094       return on capital will decline by RORFLEX %.  A larger absolute value for
   4095       this parameter will reduce the tendency of international investment flows
   4096       to respond to changes in expected rates of return.
   4097   >!
   4098    
   4099   Equation ROREXPECTED
   4100   # expected rate of return depends on the current return and investment (HT 58) #
   4101   (all,r,REG)
   4102       rore(r) = rorc(r) - RORFLEX(r) * [ke(r) - kb(r)];
   4103    
   4104    
   4105   !<
   4106       4-3. Capital Accumulation
   4107       -------------------------
   4108    
   4109       based on the theory laid out in GTAP Technical Paper #7
   4110   >!
   4111    
   4112   Variable (all,i,ENDWC_COMM)(all,r,REG)
   4113       EXPAND(i,r) # change in investment levels relative to endowment stock #;
   4114   Equation BALDWIN
   4115   # change in investment levels relative to endowment stock #
   4116   (all,i,ENDWC_COMM)(all,r,REG)
   4117       EXPAND(i,r) = qcgds(r) - qo(i,r);
   4118    
   4119    
   4120   !<
   4121       4-4. Global Bank
   4122       ----------------
   4123   >!
   4124    
   4125   Coefficient (integer,parameter)
   4126       RORDELTA
   4127       # binary coefficient to switch mechanism of allocating investment funds #;
   4128   Read
   4129       RORDELTA from file GTAPPARM header "RDLT";
   4130   !<
   4131       RORDELTA is a binary coefficient which determines the mechanism of
   4132       allocating investment funds across regions.  When RORDELTA = 1, investment
   4133       funds are allocated across regions to equate the change in the expected
   4134       rates of return (i.e., rore(r)).  When RORDELTA = 0, investment funds are
   4135       allocated across regions to maintain the existing composition of capital
   4136       stocks.
   4137   >!
   4138    
   4139   Variable
   4140       rorg # global net rate of return on capital stock #;
   4141   Variable (all,r,REG)
   4142       cgdslack(r) # slack variable for qcgds(r) #;
   4143   !<
   4144       This is exogenous, unless the user wishes to exogenously fix the level of
   4145       gross investment in a region, in which case it can be swapped with
   4146       cgdslack.
   4147   >!
   4148   Equation RORGLOBAL
   4149   # either gross investment or expected rate of return in region r (HT 59) #
   4150   (all,r,REG)
   4151       RORDELTA * rore(r)
   4152       + [1 - RORDELTA]
   4153       * [[REGINV(r) / NETINV(r)] * qcgds(r) - [VDEP(r) / NETINV(r)] * kb(r)]
   4154           = RORDELTA * rorg + [1 - RORDELTA] * globalcgds + cgdslack(r);
   4155   !<
   4156       This equation determines either gross investment or the expected rate of
   4157       return in each region, depending on the setting for the binary RORDELTA
   4158       parameter.
   4159   >!
   4160    
   4161   Equation GLOBALINV
   4162   # either expected global rate of return or global net investment (HT 11) #
   4163       RORDELTA * globalcgds + [1 - RORDELTA] * rorg
   4164           = RORDELTA
   4165           * sum(r,REG,
   4166               [REGINV(r) / GLOBINV] * qcgds(r) - [VDEP(r) / GLOBINV] * kb(r))
   4167           + [1 - RORDELTA] * sum(r,REG, [NETINV(r) / GLOBINV] * rore(r));
   4168   !<
   4169       This equation computes either the change in global net investment (when
   4170       RORDELTA = 1), or the change in the expected global rate of return on
   4171       capital (when RORDELTA = 0).
   4172   >!
   4173    
   4174    
   4175   !<
   4176       4-5. Price Index of Aggregate Global Composite Capital Goods
   4177       ------------------------------------------------------------
   4178   >!
   4179    
   4180   Equation PRICGDS
   4181   # eq'n generates a price index for the aggregate global cgds composite (HT 60) #
   4182       pcgdswld = sum(r,REG, [NETINV(r) / GLOBINV] * pcgds(r));
   4183    
   4184    
   4185   !<
   4186       4-6. Price of Saving
   4187       --------------------
   4188   >!
   4189    
   4190   Variable (all,r,REG)
   4191       psaveslack(r) # slack variable for the savings price equation #;
   4192   !<
   4193       This is exogenous under the normal closure.  However, in order to
   4194       replicate simulations from the GTAP book, where there was a single savings
   4195       price, psaveslack may be swapped with psave in order to fix the latter.
   4196       In this case, pcgdswld must be the numeraire so that each region faces the
   4197       same price of savings.
   4198   >!
   4199    
   4200   Equation SAVEPRICE
   4201   # savings price #
   4202   (all,r,REG)
   4203       psave(r)
   4204           = pcgds(r)
   4205           + sum(s,REG, [[NETINV(s) - SAVE(s)] / GLOBINV] * pcgds(s))
   4206           + psaveslack(r);
   4207   !<
   4208       In contrast to the GTAP book, the price of savings is now region-specific
   4209       and it changes at the same rate as the price of regional investment, plus
   4210       an adjustment factor which accounts for the fact that savings and
   4211       investment are not equal at the regional level.
   4212   >!
   4213    
   4214    
   4215   !<
   4216       ----------------------
   4217       5. International Trade
   4218       ----------------------
   4219    
   4220       5-1. Export Prices
   4221       5-2. Demand for Imports
   4222   >!
   4223    
   4224    
   4225   !<
   4226       5-1. Export Prices
   4227       ------------------
   4228   >!
   4229    
   4230   Variable (all,i,TRAD_COMM)(all,r,REG)
   4231       tx(i,r) # dest.-gen. change in subsidy on exports of i from r #;
   4232   Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4233       txs(i,r,s) # dest.-spec. change in subsidy on exports of i from r to s #;
   4234   !<
   4235       The variable txs captures changes in the power of bilateral export taxes.
   4236       However, the presence of a destination-generic export subsidy shift (tx)
   4237       also permits the user to swap a single export tax shock with another
   4238       target variable.  It is most naturally swapped with the variable qo to
   4239       insulate domestic producers from the world market.
   4240   >!
   4241    
   4242   Equation EXPRICES
   4243   # eq'n links agent's and world prices (HT 27) #
   4244   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4245       pfob(i,r,s) = pm(i,r) - tx(i,r) - txs(i,r,s);
   4246    
   4247   Equation TEXPRATIO
   4248   # change in ratio of export tax payments to regional income #
   4249   (all,r,REG)
   4250       100.0 * INCOME(r) * del_taxrexp(r) + TEX(r) * y(r)
   4251           = sum(i,TRAD_COMM, sum(s,REG,
   4252               VXMD(i,r,s) * [-tx(i,r) - txs(i,r,s)]
   4253               + XTAXD(i,r,s) * [pfob(i,r,s) + qxs(i,r,s)]));
   4254    
   4255    
   4256   !<
   4257       5-2. Demand for Imports
   4258       -----------------------
   4259    
   4260       Composite Imports Nest: Table 3 of Hertel and Tsigas
   4261   >!
   4262    
   4263   Variable (all,i,TRAD_COMM)(all,s,REG)
   4264       tm(i,s) # source-gen. change in tax on imports of i into s #;
   4265   Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4266       tms(i,r,s) # source-spec. change in tax on imports of i from r into s #;
   4267   !<
   4268       The variable tms captures changes in the power of bilateral import taxes.
   4269       However, the presence of a source-generic import tariff shift (tm) also
   4270       permits the user to swap a single import tariff shock with another target
   4271       variable.  In particular, to insulate domestic producers from import price
   4272       changes, it may be swapped with the relative price variable pr -- see
   4273       below.
   4274   >!
   4275    
   4276   Equation MKTPRICES
   4277   # eq'n links domestic and world prices (HT 24) #
   4278   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4279       pms(i,r,s) = tm(i,s) + tms(i,r,s) + pcif(i,r,s);
   4280    
   4281   Zerodivide (zero_by_zero) default 0;
   4282   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4283       MSHRS(i,r,s) # share of imports from r in import bill of s at mkt prices #;
   4284   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4285       MSHRS(i,r,s) = VIMS(i,r,s) / sum(k,REG, VIMS(i,k,s));
   4286   Zerodivide (zero_by_zero) off;
   4287    
   4288   Equation DPRICEIMP
   4289   # price for aggregate imports (HT 28) #
   4290   (all,i,TRAD_COMM)(all,s,REG)
   4291       pim(i,s) = sum(k,REG, MSHRS(i,k,s) * [pms(i,k,s) - ams(i,k,s)]);
   4292    
   4293   Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,r,REG)
   4294       pr(i,r) # ratio of domestic to imported prices in r #;
   4295   Equation PRICETGT
   4296   # eq'n defines target price ratio to be attained via the variable levy (HT 25) #
   4297   (all,i,TRAD_COMM)(all,s,REG)
   4298       pr(i,s) = pm(i,s) - pim(i,s);
   4299   !<
   4300       This equation defines the target price ratio to be attained via the
   4301       variable levy.  This price ratio is the ratio of domestic to average
   4302       imported goods' price.  Note that the way this price ratio is defined, it
   4303       includes intraregional imports as well.  In most applications, regions
   4304       will represent groups of individual countries.  However, in the case of
   4305       the EU, this is problematic, since recent versions of the database have
   4306       incorporated intra-EU trade flows.  Therefore, when aggregated to the EU
   4307       level, the composite import price includes both intra-EU and outside
   4308       imports.  So some modification is needed to handle the EU case.
   4309   >!
   4310    
   4311   Coefficient (parameter)(all,i,TRAD_COMM)
   4312       ESUBM(i)
   4313       # region-generic el. of sub. among imports of i in Armington structure #;
   4314   Read
   4315       ESUBM from file GTAPPARM header "ESBM";
   4316    
   4317   Equation IMPORTDEMAND
   4318   # regional demand for disaggregated imported commodities by source (HT 29)  #
   4319   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4320       qxs(i,r,s)
   4321           = -ams(i,r,s) + qim(i,s)
   4322           - ESUBM(i) * [pms(i,r,s) - ams(i,r,s) - pim(i,s)];
   4323    
   4324   Equation TIMPRATIO
   4325   # change in ratio of import tax payments to regional income #
   4326   (all,r,REG)
   4327       100.0 * INCOME(r) * del_taxrimp(r) + TIM(r) * y(r)
   4328           = sum(i,TRAD_COMM, sum(s,REG,
   4329               VIMS(i,s,r) * [tm(i,r) + tms(i,s,r)]
   4330               + MTAX(i,s,r) * [pcif(i,s,r) + qxs(i,s,r)]));
   4331    
   4332    
   4333   !<
   4334       -----------------------------------
   4335       6. International Transport Services
   4336       -----------------------------------
   4337    
   4338       6-0. Module-Specific Variables and Coefficients
   4339       6-1. Demand for Global Transport Services
   4340       6-2. Supply of Transport Services
   4341   >!
   4342    
   4343    
   4344   !<
   4345       6-0. Module-Specific Variables and Coefficients
   4346       -----------------------------------------------
   4347    
   4348       only used in this International Transport Services module
   4349   >!
   4350    
   4351   Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4352       qtmfsd(m,i,r,s) # international usage margin m on i from r to s #;
   4353   !<
   4354       International margin usage, by Margin, Freight, Source, and Destination,
   4355       i.e., the percent change in usage of m in transport of i from r to s.
   4356   >!
   4357   Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4358       atmfsd(m,i,r,s) # tech change in m's shipping of i from region r to s #;
   4359   !<
   4360       Technical progress in shipping by Margin, Freight, Source, and
   4361       Destination.  This is endogenous and driven by the following mode-,
   4362       product-, source-, and destination-specific determinants.
   4363   >!
   4364   Variable (all,m,TRAD_COMM)
   4365       atm(m) # tech change in mode m, worldwide #;
   4366   Variable (all,i,TRAD_COMM)
   4367       atf(i) # tech change shipping of i, worldwide #;
   4368   Variable (all,r,REG)
   4369       ats(r) # tech change shipping from region r #;
   4370   Variable (all,s,REG)
   4371       atd(s) # tech change shipping to s #;
   4372   Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4373       atall(m,i,r,s) # tech change in m's shipping of i from region r to s #;
   4374   Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4375       ptrans(i,r,s) # cost index for international transport of i from r to s #;
   4376   !<
   4377       average cost index for margin services used in getting i from r to s
   4378   >!
   4379   Variable (all,m,MARG_COMM)
   4380       qtm(m) # global margin usage #;
   4381   Variable (all,m,MARG_COMM)
   4382       pt(m) # price of composite margins services, type #;
   4383   !<
   4384       price index for commodity m in margin services usage
   4385   >!
   4386    
   4387   Coefficient (ge 0) (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4388       VTMFSD(m,i,r,s)
   4389       # int'l margin usage, by margin, freight, source, and destination #;
   4390   Update (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4391       VTMFSD(m,i,r,s) = pt(m) * qtmfsd(m,i,r,s);
   4392   Read
   4393       VTMFSD from file GTAPDATA header "VTWR";
   4394    
   4395   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4396       VTFSD(i,r,s) # aggregate value of svces in the shipment of i from r to s #;
   4397   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4398       VTFSD(i,r,s) = sum(m,MARG_COMM, VTMFSD(m,i,r,s));
   4399   !<
   4400       In a balanced data base, (all,i,TRAD_COMM)(all,r,REG)(all,s,REG),
   4401       VIWS(i,r,s) = VXWD(i,r,s) + VTFSD(i,r,s).
   4402   >!
   4403    
   4404   Coefficient (all,m,MARG_COMM)
   4405       VTMUSE(m) # international margin services usage, by type #;
   4406   Formula (all,m,MARG_COMM)
   4407       VTMUSE(m) = sum(i,TRAD_COMM, sum(r,REG, sum(s,REG, VTMFSD(m,i,r,s))));
   4408    
   4409   Coefficient (all,m,MARG_COMM)
   4410       VTMPROV(m) # international margin services provision #;
   4411   Formula (all,m,MARG_COMM)
   4412       VTMPROV(m) = sum(r,REG, VST(m,r));
   4413   !<
   4414       In a balanced data base, VTMPROV = VTMUSE.
   4415   >!
   4416    
   4417   Coefficient (all,r,REG)
   4418       VTRPROV(r) # international margin supply, by region #;
   4419   Formula (all,r,REG)
   4420       VTRPROV(r) = sum(m,MARG_COMM, VST(m,r));
   4421    
   4422   Coefficient
   4423       VT # international margin supply #;
   4424   Formula
   4425       VT = sum(m,MARG_COMM, sum(r,REG, VST(m,r)));
   4426    
   4427    
   4428   !<
   4429       6-1. Demand for Global Transport Services
   4430       -----------------------------------------
   4431   >!
   4432    
   4433   Equation QTRANS_MFSD
   4434   # bilateral demand for transport services  #
   4435   (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4436       qtmfsd(m,i,r,s) = qxs(i,r,s) - atmfsd(m,i,r,s);
   4437   !<
   4438       This equation computes the bilateral demand for international
   4439       transportation services.  It reflects the fact that the demand for
   4440       services along any particular route is proportional to the quantity of
   4441       merchandise shipped [i.e., QXS(i,r,s)].  It is here that we introduce the
   4442       potential for input-augmenting tech change, atmfsd(m,i,r,s), which is
   4443       commodity- and route-specific.  Thus, in the levels:
   4444               ATMFSD(m,i,r,s) * QTMFSD(m,i,r,s) = QXS(i,r,s)
   4445       where QTMFSD is the amount of composite margins services m used along this
   4446       route.  Technological improvements are reflected by atmfsd(i,r,s) > 0, and
   4447       these reduce the margins services required for this i,r,s triplet.  Tech.
   4448       change also dampens the cost of shipping, thereby lowering the CIF price
   4449       implied by a given FOB value (see 6-2).
   4450   >!
   4451    
   4452   Coefficient (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4453       VTMUSESHR(m,i,r,s) # share of i,r,s usage in global demand for m #;
   4454   Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4455       VTMUSESHR(m,i,r,s) = VTFSD(i,r,s) / VT;
   4456   Formula
   4457       (all,m,MARG_COMM: VTMUSE(m) <> 0.0)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4458       VTMUSESHR(m,i,r,s) = VTMFSD(m,i,r,s) / VTMUSE(m);
   4459    
   4460   Equation TRANS_DEMAND
   4461   # global demand for margin m #
   4462   (all,m,MARG_COMM)
   4463       qtm(m)
   4464           = sum(i,TRAD_COMM, sum(r,REG, sum(s,REG,
   4465             VTMUSESHR(m,i,r,s) * qtmfsd(m,i,r,s))));
   4466    
   4467    
   4468   !<
   4469       6-2. Supply of Transport Services
   4470       ---------------------------------
   4471   >!
   4472    
   4473   Coefficient (all,m,MARG_COMM)(all,r,REG)
   4474       VTSUPPSHR(m,r) # share of region r in global supply of margin m #;
   4475   Formula (all,m,MARG_COMM)(all,r,REG)
   4476       VTSUPPSHR(m,r) = VTRPROV(r) / VT;
   4477   Formula (all,m,MARG_COMM: VTMPROV(m) <> 0.0)(all,r,REG)
   4478       VTSUPPSHR(m,r) = VST(m,r) / VTMPROV(m);
   4479    
   4480   Equation PTRANSPORT
   4481   # generate price index for composite transportation services #
   4482   (all,m,MARG_COMM)
   4483       pt(m) = sum(r,REG, VTSUPPSHR(m,r) * pm(m,r));
   4484   !<
   4485       This equation generates a price index for transportation services based on
   4486       zero profits.  NOTE:
   4487       (1) Sales to international transportation are not subject to export tax.
   4488           This is why we base the costs to the transport sector on market prices
   4489           of the goods sold to international transportation.
   4490       (2) We assume that the supply shares for margin services are uniform
   4491           across freight, source of freight, and destination.
   4492       (cf. HT#7)
   4493   >!
   4494    
   4495   Coefficient
   4496       VTUSE # international margin services usage #;
   4497   Formula
   4498       VTUSE
   4499           = sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(r,REG, sum(s,REG,
   4500               VTMFSD(m,i,r,s)))));
   4501    
   4502   Coefficient (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4503       VTFSD_MSH(m,i,r,s) # share of margin m in cost of getting i from r to s #;
   4504   Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)
   4505           (all,s,REG: VTFSD(i,r,s) > 0.0)
   4506       VTFSD_MSH(m,i,r,s) = VTMFSD(m,i,r,s) / VTFSD(i,r,s);
   4507   Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)
   4508           (all,s,REG: VTFSD(i,r,s) = 0.0)
   4509       VTFSD_MSH(m,i,r,s) = VTMUSE(m) / VTUSE;
   4510    
   4511   Equation TRANSCOSTINDEX
   4512   #  generates flow-specific modal average cost of transport index (cf. HT7) #
   4513   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4514       ptrans(i,r,s)
   4515           = sum(m,MARG_COMM, VTFSD_MSH(m,i,r,s) * [pt(m) - atmfsd(m,i,r,s)]);
   4516    
   4517   Equation TRANSTECHANGE
   4518   #  generates flow-specific average rate of technical change #
   4519   (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4520       atmfsd(m,i,r,s) = atm(m) + atf(i) + ats(r) + atd(s) + atall(m,i,r,s);
   4521    
   4522   Equation TRANSVCES
   4523   # generate demand for regional supply of global transportation service (HT 61) #
   4524   (all,m,MARG_COMM)(all,r,REG)
   4525       qst(m,r) = qtm(m) + [pt(m) - pm(m,r)];
   4526   !<
   4527       This equation generates the international transport sector's derived
   4528       demand for regional supplies of transportation services.  It reflects a
   4529       unitary elasticity of substitution between transportation services inputs
   4530       from different regions.
   4531   >!
   4532    
   4533   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4534       VIWSCOST(i,r,s) # value of imports calculated as total cost of imports #;
   4535   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4536       VIWSCOST(i,r,s) = VXWD(i,r,s) + VTFSD(i,r,s);
   4537    
   4538   Zerodivide (zero_by_zero) default 0;
   4539   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4540       FOBSHR(i,r,s) # FOB share in VIW #;
   4541   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4542       FOBSHR(i,r,s) = VXWD(i,r,s) / VIWSCOST(i,r,s);
   4543   Zerodivide (zero_by_zero) off;
   4544    
   4545   Zerodivide (zero_by_zero) default 0;
   4546   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4547       TRNSHR(i,r,s) # transport share in VIW #;
   4548   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4549       TRNSHR(i,r,s) = VTFSD(i,r,s) / VIWSCOST(i,r,s);
   4550   Zerodivide (zero_by_zero) off;
   4551    
   4552   Equation FOBCIF
   4553   # eq'n links FOB and CIF prices for good i shipped from region r to s (HT 26') #
   4554   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   4555       pcif(i,r,s)
   4556          = FOBSHR(i,r,s) * pfob(i,r,s)
   4557          + TRNSHR(i,r,s) * ptrans(i,r,s);
   4558   !<
   4559       This equation links export and import prices for each commodity/route
   4560       triplet.  Note that technical change is embodied in ptrans(i,r,s) which is
   4561       now a cost index, as opposed to (HT 26') where it represented the price of
   4562       margins services.
   4563   >!
   4564    
