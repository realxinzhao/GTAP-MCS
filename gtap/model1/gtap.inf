   5631   !
   5632   Variable (all,i,TRAD_COMM)(all,s,REG)
   5633       vxwfob(i,s) # value of merchandise regional exports, by commodity, FOB #;
   5634   Variable (all,i,TRAD_COMM)(all,s,REG)
   5635       viwcif(i,s) # value of merchandise regional imports, by commodity, CIF #;
   5636   Variable (all,r,REG)
   5637       vxwreg(r) # value of merchandise exports, by region #;
   5638   Variable (all,r,REG)
   5639       viwreg(r) # value of merchandise imports, by region, at world prices #;
   5640    
   5641   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5642       VXW(i,r) # value of exports by comm. i and region r at FOB prices #;
   5643   Formula (all,m,MARG_COMM)(all,r,REG)
   5644       VXW(m,r) = sum(s,REG, VXWD(m,r,s)) + VST(m,r);
   5645   Formula (all,i,NMRG_COMM)(all,r,REG)
   5646       VXW(i,r) = sum(s,REG, VXWD(i,r,s));
   5647    
   5648   Coefficient (all,r,REG)
   5649       VXWREGION(r) # value of exports by region r at FOB prices #;
   5650   Formula (all,r,REG)
   5651       VXWREGION(r) = sum(i,TRAD_COMM, VXW(i,r));
   5652    
   5653   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   5654       VIW(i,s) # value of commodity imports i into s at CIF prices #;
   5655   Formula (all,i,TRAD_COMM)(all,s,REG)
   5656       VIW(i,s) = sum(r,REG, VIWS(i,r,s));
   5657   Coefficient (all,r,REG)
   5658       VIWREGION(r) # value of commodity imports by region r at CIF prices #;
   5659   Formula (all,r,REG)
   5660       VIWREGION(r) = sum(i,TRAD_COMM, VIW(i,r));
