   5220   !<
   5221       8-5. Computation of Regional Income
   5222       -----------------------------------
   5223   >!
   5224    
   5225   Coefficient (all,r,REG)
   5226       FY(r) # primary factor income in r net of depreciation #;
   5227   Formula (all,r,REG)
   5228       FY(r) = sum(i,ENDW_COMM, VOM(i,r)) - VDEP(r);
   5229   Variable (all,r,REG)
   5230       fincome(r) # factor income at market prices net of depreciation #;
   5231   Equation FACTORINCOME
   5232   # factor income at market prices net of depreciation #
   5233   (all,r,REG)
   5234       FY(r) * fincome(r)
   5235           = sum(i,ENDW_COMM, VOM(i,r) * [pm(i,r) + qo(i,r)])
   5236           - VDEP(r)  * [pcgds(r) + kb(r)];
   5237    
   5238   Variable (change)(all,r,REG)
   5239       del_indtaxr(r) # change in ratio of indirect taxes to INCOME in r #;
   5240   Equation DINDTAXRATIO
   5241   # change in ratio of indirect taxes to INCOME in r #
   5242   (all,r,REG)
   5243       del_indtaxr(r)
   5244           = del_taxrpc(r) + del_taxrgc(r) + del_taxriu(r) + del_taxrfu(r)
   5245           + del_taxrout(r) + del_taxrexp(r) + del_taxrimp(r);
   5246    
   5247   Variable (change)(all,r,REG)
   5248       del_ttaxr(r) # change in ratio of taxes to INCOME in r #;
   5249   Equation DTAXRATIO
   5250   # change in ratio of taxes to INCOME in r #
   5251   (all,r,REG)
   5252       del_ttaxr(r)
   5253           = del_taxrpc(r) + del_taxrgc(r) + del_taxriu(r) + del_taxrfu(r)
   5254           + del_taxrout(r) + del_taxrexp(r) + del_taxrimp(r) + del_taxrinc(r);
   5255   !<
   5256      This variable can be swapped with the commodity- and source-generic
   5257      consumption tax shift, tp(r), in order to generate a tax replacement
   5258      scenario, whereby taxes remain a constant share of national income.
   5259   >!
   5260    
   5261   Variable (all,r,REG)
   5262       incomeslack(r) # slack variable in the expression for regional income #;
   5263   !<
   5264       This is exogenous, unless the user wishes to fix regional income.
   5265   >!
   5266    
   5267   Coefficient (all,r,REG)
   5268       INDTAX(r) # indirect tax receipts in r #;
   5269   Formula (all,r,REG)
   5270       INDTAX(r) = TPC(r) + TGC(r) + TIU(r) + TFU(r) + TOUT(r) + TEX(r) + TIM(r);
   5271    
   5272   Equation REGIONALINCOME
   5273   # regional income = sum of primary factor income and indirect tax receipts #
   5274   (all,r,REG)
   5275       INCOME(r) * y(r)
   5276           = FY(r) * fincome(r)
   5277           + 100.0 * INCOME(r) * del_indtaxr(r)
   5278           + INDTAX(r) * y(r)
   5279           + INCOME(r) * incomeslack(r);
   5280   !<
   5281       This equation computes regional income as the sum of primary factor
   5282       payment and indirect tax receipts.  The first term computes the change in
   5283       endowment income, net of depreciation.  The second and third terms compute
   5284       the change in ordinary indirect tax revenue.  The fourth term computes the
   5285       revenues/expenditures from carbon emissions trading.  The fifth term
   5286       computes revenue from carbon taxes.
   5287   >!
   5288    
   5289    
   5290   !<
   5291       8-6. Regional Household Demand System
   5292       -------------------------------------
   5293   >!
   5294    
   5295   Variable (all,r,REG)
   5296       dpav(r) # average distribution parameter shift, for EV calc. #;
   5297   Equation DPARAV
   5298   # average distribution parameter shift #
   5299   (all,r,REG)
   5300       dpav(r)
   5301           = XSHRPRIV(r) * dppriv(r)
   5302           + XSHRGOV(r) * dpgov(r)
   5303           + XSHRSAVE(r) * dpsave(r);
   5304    
   5305   Equation UTILITELASTIC
   5306   # elasticity of cost of utility wrt utility #
   5307   (all,r,REG)
   5308       uelas(r) = XSHRPRIV(r) * uepriv(r) - dpav(r);
   5309    
   5310   Equation PRIVCONSEXP
   5311   # private consumption expenditure #
   5312   (all,r,REG)
   5313       yp(r) - y(r) = -[uepriv(r) - uelas(r)] + dppriv(r);
   5314    
   5315   Equation GOVCONSEXP
   5316   # government consumption expenditure #
   5317   (all,r,REG)
   5318       yg(r) - y(r) = uelas(r) + dpgov(r);
   5319    
   5320   Equation SAVING
   5321   # saving #
   5322   (all,r,REG)
   5323       psave(r) + qsave(r) - y(r) = uelas(r) + dpsave(r);
   5324    
   5325    
   5326   !<
   5327       8-7. Aggregate Utility
   5328       ----------------------
   5329   >!
   5330   Equation PRICEINDEXREG
   5331       # price index for disposition of income by regional household #
   5332       (all,r,REG)
   5333       p(r)
   5334           = XSHRPRIV(r) * ppriv(r)
   5335           + XSHRGOV(r) * pgov(r)
   5336           + XSHRSAVE(r) * psave(r);
   5337    
   5338   Variable (all,r,REG)
   5339       au(r) # input-neutral shift in utility function #;
   5340   Variable (all,r,REG)
   5341       dpsum(r) # sum of the distribution parameters #;
   5342    
   5343   Coefficient (all,r,REG)
   5344       DPARSUM(r) # sum of distribution parameters #;
   5345   Read
   5346       DPARSUM from file GTAPDATA header "DPSM";
   5347   Update (all,r,REG)
   5348       DPARSUM(r) = dpsum(r);
   5349    
   5350   Coefficient (all,r,REG)
   5351       UTILELAS(r) # elasticity of cost of utility wrt utility #;
   5352   Formula (all,r,REG)
   5353       UTILELAS(r)
   5354           = [UELASPRIV(r) * XSHRPRIV(r) + XSHRGOV(r) + XSHRSAVE(r)] / DPARSUM(r);
   5355    
   5356   Coefficient (all,r,REG)
   5357       DPARPRIV(r) # private consumption distribution parameter #;
   5358   Formula (all,r,REG)
   5359       DPARPRIV(r) = UELASPRIV(r) * XSHRPRIV(r) / UTILELAS(r);
   5360    
   5361   Coefficient (all,r,REG)
   5362       DPARGOV(r) # government consumption distribution parameter #;
   5363   Formula (all,r,REG)
   5364       DPARGOV(r) = XSHRGOV(r) / UTILELAS(r);
   5365    
   5366   Coefficient (all,r,REG)
   5367       DPARSAVE(r) # saving distribution parameter #;
   5368   Formula (all,r,REG)
   5369       DPARSAVE(r) = XSHRSAVE(r) / UTILELAS(r);
   5370    
   5371   Coefficient (all,r,REG)
   5372       UTILPRIV(r) # utility from private consumption #;
   5373   Formula (initial)(all,r,REG)
   5374       UTILPRIV(r) = 1.0;
   5375   Update (all,r,REG)
   5376       UTILPRIV(r) = up(r);
   5377    
   5378   Coefficient (all,r,REG)
   5379       UTILGOV(r) # utility from government consumption #;
   5380   Formula (initial)(all,r,REG)
   5381       UTILGOV(r) = 1.0;
   5382   Update (all,r,REG)
   5383       UTILGOV(r) = ug(r);
   5384    
   5385   Coefficient (all,r,REG)
   5386       UTILSAVE(r) # utility from saving #;
   5387   Formula (initial)(all,r,REG)
   5388       UTILSAVE(r) = 1.0;
   5389   Update (change) (all,r,REG)
   5390       UTILSAVE(r) = [[qsave(r) - pop(r)] / 100] * UTILSAVE(r);
   5391    
   5392   Variable (all,r,REG)
   5393       u(r) # per capita utility from aggregate hhld expend. in region r #;
   5394   Equation UTILITY
   5395   # regional household utility #
   5396   (all,r,REG)
   5397       u(r)
   5398           = au(r)
   5399           + DPARPRIV(r) * loge(UTILPRIV(r)) * dppriv(r)
   5400           + DPARGOV(r) * loge(UTILGOV(r)) * dpgov(r)
   5401           + DPARSAVE(r) * loge(UTILSAVE(r)) * dpsave(r)
   5402           + [1.0 / UTILELAS(r)] * [y(r) - pop(r) - p(r)];
   5403    
   5404   Equation DISTPARSUM
   5405   # sum of the distribution parameters #
   5406   (all,r,REG)
   5407       DPARSUM(r) * dpsum(r)
   5408           = DPARPRIV(r) * dppriv(r)
   5409           + DPARGOV(r) * dpgov(r)
   5410           + DPARSAVE(r) * dpsave(r);
   5411    
   5412    
   5413   !<
   5414       -------------------------
   5415       9. Equilibrium Conditions
   5416       -------------------------
   5417    
   5418       9-1. Market Clearing Conditions
   5419       9-2. Walras' Law
   5420   >!
   5421    
   5422    
   5423   !<
   5424       9-1. Market Clearing Conditions
   5425       -------------------------------
   5426   >!
   5427   Zerodivide (zero_by_zero) default 0;
   5428    
   5429   Coefficient (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   5430       SHRDFM(i,j,r) # share of dom. prod. i used by sector j in r at mkt prices #;
   5431   Formula (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   5432       SHRDFM(i,j,r) = VDFM(i,j,r) / VDM(i,r);
   5433    
   5434   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5435       SHRDPM(i,r) # share of domestic prod. of i used by private hhlds in r #;
   5436   Formula (all,i,TRAD_COMM)(all,r,REG)
   5437       SHRDPM(i,r) = VDPM(i,r) / VDM(i,r);
   5438    
   5439   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5440       SHRDGM(i,r) # share of imports of i used by gov't hhlds in r #;
   5441   Formula (all,i,TRAD_COMM)(all,r,REG)
   5442       SHRDGM(i,r) = VDGM(i,r) / VDM(i,r);
   5443   Zerodivide (zero_by_zero) off;
   5444    
   5445   Variable (orig_level=VDM)(all,i,TRAD_COMM)(all,r,REG)
   5446       qds(i,r) # domestic sales of commodity i in r #;
   5447   Equation MKTCLDOM
   5448   # eq'n assures market clearing for domestic sales (HT 3) #
   5449   (all,i,TRAD_COMM)(all,r,REG)
   5450       qds(i,r)
   5451           = sum(j,ALL_INDS, SHRDFM(i,j,r) * qfd(i,j,r))
   5452           + SHRDPM(i,r) * qpd(i,r)
   5453           + SHRDGM(i,r) * qgd(i,r);
   5454    
   5455   Zerodivide (zero_by_zero) default 0;
   5456    
   5457   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5458       SHRDM(i,r) # share of domestic sales of i in r #;
   5459   Formula (all,i,TRAD_COMM)(all,r,REG)
   5460       SHRDM(i,r) = VDM(i,r) / VOM(i,r);
   5461    
   5462   Coefficient (all,m,MARG_COMM)(all,r,REG)
   5463       SHRST(m,r) # share of sales of m to global transport services in r #;
   5464   Formula (all,m,MARG_COMM)(all,r,REG)
   5465       SHRST(m,r) = VST(m,r) / VOM(m,r);
   5466    
   5467   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5468       SHRXMD(i,r,s) # share of export sales of i to s in r #;
   5469   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5470       SHRXMD(i,r,s) = VXMD(i,r,s) / VOM(i,r);
   5471    
   5472    
   5473   Variable (all,i,TRAD_COMM)(all,r,REG)
   5474       tradslack(i,r) # slack variable in tradeables market clearing condition #;
   5475   !<
   5476       This is exogenous unless the user wishes to specify the price of
   5477       tradeables exogenously, in which case the analysis becomes partial
   5478       equilibrium and walraslack must be exogenized.
   5479   >!
   5480    
   5481   Equation MKTCLTRD_MARG
   5482   # eq'n assures market clearing for margins commodities (HT 1) #
   5483   (all,m,MARG_COMM)(all,r,REG)
   5484       qo(m,r)
   5485           = SHRDM(m,r) * qds(m,r)
   5486           + SHRST(m,r) * qst(m,r)
   5487           + sum(s,REG, SHRXMD(m,r,s) * qxs(m,r,s))
   5488           + tradslack(m,r);
   5489   Equation MKTCLTRD_NMRG
   5490   # eq'n assures market clearing for the non-margins commodities (HT 1) #
   5491   (all,i,NMRG_COMM)(all,r,REG)
   5492       qo(i,r)
   5493           = SHRDM(i,r) * qds(i,r)
   5494           + sum(s,REG, SHRXMD(i,r,s) * qxs(i,r,s))
   5495           + tradslack(i,r);
   5496    
   5497   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5498       VIM(i,r) # value of imports of commodity i in r at domestic market prices #;
   5499   Formula (all,i,TRAD_COMM)(all,r,REG)
   5500       VIM(i,r) = sum(j,ALL_INDS, VIFM(i,j,r)) + VIPM(i,r) + VIGM(i,r);
   5501    
   5502   Coefficient (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   5503       SHRIFM(i,j,r) # share of import i used by sector j in r  #;
   5504   Formula (all,i,TRAD_COMM)(all,j,ALL_INDS)(all,r,REG)
   5505       SHRIFM(i,j,r) = VIFM(i,j,r) / VIM(i,r);
   5506    
   5507   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5508       SHRIPM(i,r) # share of import i used by private hhlds in r #;
   5509   Formula (all,i,TRAD_COMM)(all,r,REG)
   5510       SHRIPM(i,r) = VIPM(i,r) / VIM(i,r);
   5511    
   5512   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5513       SHRIGM(i,r) # the share of import i used by gov't hhlds in r #;
   5514   Formula (all,i,TRAD_COMM)(all,r,REG)
   5515       SHRIGM(i,r) = VIGM(i,r) / VIM(i,r);
   5516    
   5517   Equation MKTCLIMP
   5518   # eq'n assures mkt clearing for imported goods entering each region (HT 2) #
   5519   (all,i,TRAD_COMM)(all,r,REG)
   5520       qim(i,r)
   5521           = sum(j,ALL_INDS, SHRIFM(i,j,r) * qfm(i,j,r))
   5522           + SHRIPM(i,r) * qpm(i,r)
   5523           + SHRIGM(i,r) * qgm(i,r);
   5524    
   5525   Coefficient (all,i,ENDWM_COMM)(all,j,ALL_INDS)(all,r,REG)
   5526       SHREM(i,j,r) # share of mobile endowment i used by sector j at mkt prices #;
   5527   Formula (all,i,ENDWM_COMM)(all,j,ALL_INDS)(all,r,REG)
   5528       SHREM(i,j,r) = VFM(i,j,r) / VOM(i,r);
   5529   Zerodivide (zero_by_zero) off;
   5530   Equation MKTCLENDWM
   5531   # eq'n assures mkt clearing for perfectly mobile endowments in each r (HT 4) #
   5532   (all,i,ENDWM_COMM)(all,r,REG)
   5533       qo(i,r) = sum(j,ALL_INDS, SHREM(i,j,r) * qfe(i,j,r)) + endwslack(i,r);
   5534   !<
   5535       This equation assures market clearing for perfectly mobile endowments (HT 4)
   5536   >!
   5537    
   5538   Equation MKTCLENDWS
   5539   # eq'n assures mkt clearing for imperfectly mobile endowments in each r (HT 5)#
   5540   (all,i,ENDWS_COMM)(all,j,ALL_INDS)(all,r,REG)
   5541       qoes(i,j,r) = qfe(i,j,r);
   5542   !<
   5543       This equation assures market clearing for sluggish endowments (HT 5)
   5544   >!
   5545    
   5546    
   5547   !<
   5548       9-2. Walras' Law
   5549       ----------------
   5550   >!
   5551    
   5552   Variable
   5553       walras_sup # supply in omitted market--global supply of cgds composite #;
   5554   Equation WALRAS_S
   5555   # Extra eq'n computes change in supply in the omitted market. #
   5556       walras_sup = pcgdswld + globalcgds;
   5557   !<
   5558       This is an extra equation which simply computes change in supply in the
   5559       omitted market.  (modified from HT 12 to reflect the value as opposed to
   5560       quantity change, in light of the new treatment of psave(r))
   5561   >!
   5562    
   5563   Variable
   5564       walras_dem # demand in the omitted market--global demand for savings #;
   5565   Equation WALRAS_D
   5566   # Extra eq'n computes change in demand in the omitted market. #
   5567       GLOBINV * walras_dem = sum(r,REG, SAVE(r) * [psave(r) + qsave(r)]);
   5568   !<
   5569       This is an extra equation which simply computes change in demand in the
   5570       omitted market.  (modified from HT 13 to reflect the value as opposed to
   5571       quantity change)
   5572   >!
   5573    
   5574   Variable
   5575       walraslack # slack variable in the omitted market #;
   5576   !<
   5577       This is endogenous under normal, GE closure.  If the GE links are broken,
   5578       then this must be swapped with the numeraire, thereby forcing global
   5579       savings to explicitly equal global investment.
   5580   >!
   5581   Equation WALRAS
   5582   # Check Walras' Law.  Value of "walraslack" should be zero. (HT 14) #
   5583       walras_sup = walras_dem + walraslack;
   5584   !<
   5585       This equation checks Walras' Law.  The value of walraslack should be zero
   5586       in any GE simulation.  (HT 14)
   5587   >!
