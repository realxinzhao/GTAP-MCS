   2191       3-1. Factor Employment Taxation
   2192       -------------------------------
   2193   >!
   2194    
   2195   Variable (all,i,ENDW_COMM)(all,j,ALL_INDS)(all,r,REG)
   2196       tf(i,j,r) # tax on primary factor i used by j in region r #;
   2197   Equation MPFACTPRICE # eq'n links domestic and firm demand prices (HT 16) #
   2198   (all,i,ENDWM_COMM)(all,j,ALL_INDS)(all,r,REG)
   2199       pfe(i,j,r) = tf(i,j,r) + pm(i,r);
   2200    
   2201   Equation SPFACTPRICE # eq'n links domestic and firm demand prices (HT 17) #
   2202   (all,i,ENDWS_COMM)(all,j,ALL_INDS)(all,r,REG)
   2203       pfe(i,j,r) = tf(i,j,r) + pmes(i,j,r);
   2204    
   2205   Equation TFURATIO
   2206   # change in ratio of tax payments on factor usage to regional income #
   2207   (all,r,REG)
   2208       100.0 * INCOME(r) * del_taxrfu(r) + TFU(r) * y(r)
   2209           = sum(i,ENDWM_COMM, sum(j,ALL_INDS, VFAD(i,j,r) * tf(i,j,r)))
   2210           + sum(i,ENDWM_COMM, sum(j,ALL_INDS,
   2211               ETAX(i,j,r) * [pm(i,r) + qfe(i,j,r)]))
   2212           + sum(i,ENDWS_COMM, sum(j,ALL_INDS, VFAD(i,j,r) * tf(i,j,r)))
   2213           + sum(i,ENDWS_COMM, sum(j,ALL_INDS,
   2214               ETAX(i,j,r) * [pmes(i,j,r) + qfe(i,j,r)]));
